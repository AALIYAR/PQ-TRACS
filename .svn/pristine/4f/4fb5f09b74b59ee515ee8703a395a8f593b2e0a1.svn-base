  <SQLQuery>
    <![CDATA[SELECT PrmOpportunity.ID, 
       PrmOpportunity.CONTENT_OWNER_ID, 
       PrmOpportunity.OPPORTUNITY_NO, 
       PrmOpportunity.NAME, 
       PrmOpportunity.DESCRIPTION, 
       PrmOpportunity.OPPORTUNITY_TYPE, 
       PrmOpportunity.CURRENT_STATUS, 
       PrmOpportunity.AGREEMENT_RECEIVED_DATE, 
       PrmOpportunity.CURRENT_PRE_STATUS, 
       PrmOpportunity.CURRENT_CATALOGING_STATUS, 
       PrmOpportunity.CURRENT_AGREEMENT_STATUS, 
       PrmOpportunity.SUMMARY, 
       PrmOpportunity.AUTO_ADD_TARGETS_FLAG, 
       PrmOpportunity.CREATED_BY, 
       PrmOpportunity.CREATED_DATE, 
       PrmOpportunity.LAST_UPDATED_BY, 
       PrmOpportunity.LAST_UPDATED_DATE,
       TraxContentOwner.CONTENT_OWNER_NO ACCOUNT_NUMBER,
       TraxContentOwner.NAME ACCOUNT_NAME,
       TraxContentOwner.NAME ACCOUNT_NAME_2,
       TraxContentOwner.SITE ACCOUNT_SITE,
       TraxContentOwner.KEY_ACCOUNT_LEVEL,
       JhsUsers.DISPLAY_NAME AS Alliance_Manager,  
       (select count(distinct TraxTitleCat.ID) 
            from TRAX_TITLE TraxTitleCat
                inner join prm_targeted_product_title tptcat
                    inner join prm_opportunity_target potcat
                        on tptcat.id = potcat.PRM_TARGETED_PRODUCT_TITLE_ID
                    on TraxTitleCat.id = tptcat.TITLE_ID
            where potcat.PRM_OPPORTUNITY_ID = PrmOpportunity.ID
            and TraxTitleCat.catalog_number is not null) AS CATALOGUED_COUNT,
       (select count(distinct TraxTitleNoCat.ID) 
            from TRAX_TITLE TraxTitleNoCat
                inner join prm_targeted_product_title tptnocat
                    inner join prm_opportunity_target potnocat
                        on tptnocat.id = potnocat.PRM_TARGETED_PRODUCT_TITLE_ID
                    on TraxTitleNoCat.id = tptnocat.TITLE_ID
            where potnocat.PRM_OPPORTUNITY_ID = PrmOpportunity.ID
            and TraxTitleNoCat.catalog_number is null) AS UNCATALOGUED_COUNT,
       LookupCode.LOOKUP_CODE_SET_ID
FROM PRM_OPPORTUNITY PrmOpportunity
    INNER JOIN TRAX_CONTENT_OWNER TraxContentOwner
        ON PrmOpportunity.CONTENT_OWNER_ID = TraxContentOwner.ID
    LEFT OUTER JOIN PRM_OPPORTUNITY_ALLIANCE_MGR PrmOpportunityAllianceMgr
        INNER JOIN PRM_ALLIANCE_MANAGER PrmAllianceManager
            INNER JOIN JHS_USERS JhsUsers
                ON PrmAllianceManager.JHS_USERS_ID = JhsUsers.ID
            ON PrmOpportunityAllianceMgr.PRM_ALLIANCE_MANAGER_ID = PrmAllianceManager.ID
            AND PrmAllianceManager.USER_TYPE in ('AM', 'AST')
        ON PrmOpportunity.ID = PrmOpportunityAllianceMgr.PRM_OPPORTUNITY_ID
        AND PrmOpportunityAllianceMgr.END_DATE IS NULL
    INNER JOIN TRAX_LOOKUP_CODE LookupCode
        ON PrmOpportunity.OPPORTUNITY_TYPE = LookupCode.INTERNAL_VALUE
        AND LookupCode.LOOKUP_CODE_SET_ID IN (210, 273)]]>
  </SQLQuery>
