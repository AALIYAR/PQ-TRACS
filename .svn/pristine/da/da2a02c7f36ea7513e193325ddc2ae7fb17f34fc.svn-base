                                          rows="#{bindings.CAActivityDashboardTable.hints.Type.displayHeight}"
                                          columns="#{bindings.CAActivityDashboardTable.hints.Type.displayWidth}"
                                          maximumLength="40" readOnly="true"
                                          shortDesc="#{bindings.CAActivityDashboardTable.hints.Type.tooltip}"
                                          rendered="#{adfFacesContext.outputMode eq 'printable'}">
