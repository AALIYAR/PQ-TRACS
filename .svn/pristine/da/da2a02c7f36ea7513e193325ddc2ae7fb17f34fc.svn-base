                                              id="ol3"/>
                            </f:facet>
                            <f:facet name="filter">
                              <af:inputDate value="#{status.filterCriteria.DueDate}"
                                            id="id0">
                                <af:convertDateTime pattern="#{nls.datepattern}"/>
                              </af:inputDate>
                            </f:facet>
                            <af:inputDate id="CAcDueDate"
                                          value="#{row.bindings.DueDate.inputValue}"
                                          label="#{bindings.CAActivityDashboardTable.hints.DueDate.label}"
                                          required="#{bindings.CAActivityDashboardTable.hints.DueDate.mandatory}"
                                          readOnly="true"
                                          shortDesc="#{bindings.CAActivityDashboardTable.hints.DueDate.tooltip}">
                              <af:convertDateTime pattern="#{nls.datepattern}"/>
                            </af:inputDate>
                          </af:column>
                          <af:column id="CAcUserTypeCol" sortable="true"
                                     filterable="true" noWrap="true" width="120"
                                     sortProperty="UserType" rowHeader="false"
                                     filterFeatures="caseInsensitive">
                            <f:facet name="header">
                              <af:outputLabel value="#{bindings.CAActivityDashboardTable.hints.UserType.label}"
