<?xml version='1.0' encoding="UTF-8"?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1"
          xmlns:h="http://java.sun.com/jsf/html"
          xmlns:f="http://java.sun.com/jsf/core"
          xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
          xmlns:trh="http://myfaces.apache.org/trinidad/html"
          xmlns:tr="http://myfaces.apache.org/trinidad"
          xmlns:dvt="http://xmlns.oracle.com/dss/adf/faces">
  <jsp:directive.page contentType="text/html;charset=UTF-8"/>
  <af:pageTemplate id="pt"
                   viewId="/common/pageTemplates/JhsRegionTemplate.jspx">
    <f:facet name="pageContent">
      <af:panelStretchLayout topHeight="auto" id="psl0" partialTriggers=""
                             rendered="#{jhsUserRoles['CO, ADMIN']}">
        <f:facet name="top">
          <af:panelGroupLayout layout="scroll" id="CADTopLc" partialTriggers=""
                               rendered="#{jhsUserRoles['CO, ADMIN']}">
            <f:facet name="separator">
              <af:spacer width="10" height="10" id="sp0"/>
            </f:facet>
            <!-- Pushed "disabled" on stretchContext, now: enabled (default), disabled (default/pageComponent/groupContent.vm) -->
            <af:group id="g0"></af:group>
            <af:switcher rendered="#{bindings.CADashboardIterator.findMode!='true'}"
                         id="CADSw" defaultFacet="hasRows"
                         facetName="#{( bindings.CADashboardIterator.currentRow!=null ) ? 'hasRows' : 'noRows'}">
              <f:facet name="noRows">
                <af:outputFormatted value="#{nls['NO_ROWS_FOUND']}"
                                    styleUsage="pageStamp" id="CADNoRowsFound"/>
              </f:facet>
              <f:facet name="hasRows">
                <!-- Pushed "disabled" on stretchContext, now: enabled (default), disabled (default/pageComponent/groupContent.vm), disabled (default/pageComponent/formGroup.vm) -->
                <af:panelGroupLayout id="CADFC" layout="vertical">
                  <f:facet name="separator">
                    <af:spacer width="10" height="10" id="sp1"/>
                  </f:facet>
                  <af:panelFormLayout rows="1" labelAlignment="start"
                                      maxColumns="2" id="CADFI">
                    <af:inputText id="CADDisplayName"
                                  value="#{bindings.CADDisplayName.inputValue}"
                                  label="Welcome"
                                  required="#{bindings.CADDisplayName.hints.mandatory}"
                                  rows="#{bindings.CADDisplayName.hints.displayHeight}"
                                  columns="#{bindings.CADDisplayName.hints.displayWidth}"
                                  maximumLength="240" readOnly="true"
                                  shortDesc="#{bindings.CADDisplayName.hints.tooltip}">
                      <f:validator binding="#{bindings.CADDisplayName.validator}"/>
                    </af:inputText>
                  </af:panelFormLayout>
                </af:panelGroupLayout>
                <!-- Popped "disabled" from stretchContext, now: enabled (default), disabled (default/pageComponent/groupContent.vm) -->
              </f:facet>
            </af:switcher>
            <!-- Popped "disabled" from stretchContext, now: enabled (default) -->
            <af:spacer width="10" height="10" id="sp2"/>
          </af:panelGroupLayout>
        </f:facet>
        <f:facet name="center">
          <af:panelDashboard id="CADDb" columns="2" rowHeight="300px">
            <af:panelBox text="#{nls['TABLE_TITLE_CAACTIVITYDASHBOARD']}"
                         id="CAcPb">
              <f:facet name="toolbar"/>
              <af:panelStretchLayout topHeight="auto" id="CAcTopLc"
                                     partialTriggers=" CADTopLc qsAreaCAcSearchOptions"
                                     rendered="#{jhsUserRoles['CO, ADMIN']}">
                <f:facet name="top">
                  <!-- Pushed "disabled" on stretchContext, now: enabled (default), disabled (tracs_templates/pageComponent/dashboardContent.vm) -->
                  <af:group id="g1">
                    <af:panelGroupLayout id="modelSearchAreaCAc"
                                         rendered="#{!pageFlowScope.createModes.CreateCAActivityDashboard and !pageFlowScope.hideSearchArea}"
                                         partialTriggers="">
                      <f:attribute name="JhsIgnoreChanges" value="true"/>
                      <af:quickQuery id="qsAreaCAcSearchOptions"
                                     label="#{nls['FILTER_BY']}"
                                     value="#{bindings.CAActivityDashboardIteratorActivityDashboardViewCriteriaQuery.queryDescriptor}"
                                     model="#{bindings.CAActivityDashboardIteratorActivityDashboardViewCriteriaQuery.queryModel}"
                                     queryListener="#{bindings.CAActivityDashboardIteratorActivityDashboardViewCriteriaQuery.processQuery}"></af:quickQuery>
                    </af:panelGroupLayout>
                    <af:spacer width="10" height="10" id="sp3"/>
                  </af:group>
                  <!-- Popped "disabled" from stretchContext, now: enabled (default) -->
                </f:facet>
                <f:facet name="center">
                  <af:panelStretchLayout topHeight="0" id="psl1">
                    <f:facet name="top"></f:facet>
                    <f:facet name="center">
                      <af:panelCollection id="CAcPC" featuresOff=" "
                                          styleClass="AFStretchWidth">
                        <f:facet name="menus">
                          <af:menu textAndAccessKey="#{nls['ACTION_MENU_LABEL']}"
                                   id="menu0">
                            <af:menu textAndAccessKey="#{nls['GO_TO_MENU_LABEL']}"
                                     id="menu1">
                              <af:group id="CActbb"
                                        rendered="#{ bindings.CAActivityDashboardIterator.currentRow!=null }">
                                <af:commandMenuItem actionListener="#{bindings.FirstCAActivityDashboard.execute}"
                                                    id="CAcFirst"
                                                    partialSubmit="true"
                                                    icon="/afr/shuttleleftall_ena.png"
                                                    textAndAccessKey="#{nls['FIRST_ROW']}"></af:commandMenuItem>
                                <af:commandMenuItem actionListener="#{bindings.PreviousCAActivityDashboard.execute}"
                                                    icon="/afr/shuttleleft_ena.png"
                                                    textAndAccessKey="#{nls['PREVIOUS_ROW']}"
                                                    id="CAcPrevious"
                                                    partialSubmit="true"></af:commandMenuItem>
                                <af:commandMenuItem actionListener="#{bindings.NextCAActivityDashboard.execute}"
                                                    icon="/afr/shuttleright_ena.png"
                                                    textAndAccessKey="#{nls['NEXT_ROW']}"
                                                    id="CAcNext"
                                                    partialSubmit="true"></af:commandMenuItem>
                                <af:commandMenuItem actionListener="#{bindings.LastCAActivityDashboard.execute}"
                                                    icon="/afr/shuttlerightall_ena.png"
                                                    textAndAccessKey="#{nls['LAST_ROW']}"
                                                    id="CAcLast"
                                                    partialSubmit="true"></af:commandMenuItem>
                              </af:group>
                            </af:menu>
                            <af:commandMenuItem partialSubmit="true"
                                                textAndAccessKey="#{nls['EXPORT']}"
                                                icon="/jheadstart/images/excel.png"
                                                immediate="true" id="cmi0">
                              <af:exportCollectionActionListener exportedId="CAcTab"
                                                                 type="excelHTML"
                                                                 filename="CAActivityDashboard.xls"/>
                            </af:commandMenuItem>
                          </af:menu>
                        </f:facet>
                        <f:facet name="toolbar">
                          <af:toolbar id="tbar0">
                            <af:commandImageLink immediate="true"
                                                 partialSubmit="true" id="cl0"
                                                 shortDesc="#{nls['EXPORT']}"
                                                 icon="/jheadstart/images/excel.png">
                              <af:exportCollectionActionListener exportedId="CAcTab"
                                                                 type="excelHTML"
                                                                 filename="CAActivityDashboard.xls"/>
                            </af:commandImageLink>
                            <af:commandToolbarButton text="Print" id="CAcPrint"
                                                     shortDesc="Opens a new tab with printer friendly format.">
                              <af:showPrintablePageBehavior/>
                            </af:commandToolbarButton>
                          </af:toolbar>
                        </f:facet>
                        <f:facet name="statusbar">
                          <af:toolbar>
                            <af:outputText value="#{bindings.CAActivityDashboardIterator.estimatedRowCount} rows found"/>
                          </af:toolbar>
                        </f:facet>
                        <af:table id="CAcTab" contentDelivery="immediate"
                                  value="#{bindings.CAActivityDashboardTable.collectionModel}"
                                  columnStretching="blank"
                                  rowBandingInterval="0" editingMode="editAll"
                                  columnSelection="multiple"
                                  contextMenuSelect="true" rowSelection="single"
                                  selectionListener="#{pageFlowScope.CAActivityDashboardTable.makeCurrent}"
