                                  filterModel="#{bindings.CAActivityDashboardIteratorActivityDashboardViewCriteriaQuery.queryDescriptor}"
                                  filterVisible="true"
                                  binding="#{pageFlowScope.CAActivityDashboardTable.table}"
                                  partialTriggers=":::qsAreaCAcSearchOptions :::CAcPC ::CAcFirst ::CAcPrevious ::CAcNext ::CAcLast"
                                  styleClass="AFStretchWidth"
