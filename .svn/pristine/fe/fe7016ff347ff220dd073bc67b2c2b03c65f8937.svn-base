                                             icon="/afr/shuttleright_ena.png"
                                             shortDesc="#{nls['NEXT_ROW']}"
                                             id="OppNext" partialSubmit="true"
                                             rendered="#{bindings.OpportunitiesIterator.rangeStart+bindings.OpportunitiesIterator.currentRowIndexInRange+1 lt bindings.OpportunitiesIterator.estimatedRowCount}"></af:commandToolbarButton>
                    <af:image source="/afr/shuttleright_dis.png"
                              rendered="#{!(bindings.OpportunitiesIterator.rangeStart+bindings.OpportunitiesIterator.currentRowIndexInRange+1 lt bindings.OpportunitiesIterator.estimatedRowCount)}"
