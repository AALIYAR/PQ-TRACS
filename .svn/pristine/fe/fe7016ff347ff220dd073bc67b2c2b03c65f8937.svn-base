              <af:toolbox id="tbox0">
                <af:toolbar id="tbar0">
                  <af:panelGroupLayout id="Oppfbb" layout="horizontal"
                                       rendered="#{ (bindings.OpportunitiesIterator.viewObject!=null and bindings.OpportunitiesIterator.viewObject.currentRow!=null) and bindings.OpportunitiesIterator.estimatedRowCount>1 and !pageFlowScope.createModes.CreateOpportunities and !pageFlowScope.hideFormBrowseButtons}"
                                       partialTriggers="OppFirst OppPrevious OppNext OppLast">
                    <af:commandToolbarButton actionListener="#{bindings.FirstOpportunities.execute}"
                                             id="OppFirst" partialSubmit="true"
                                             icon="/afr/shuttleleftall_ena.png"
                                             shortDesc="#{nls['FIRST_ROW']}"
                                             rendered="#{bindings.OpportunitiesIterator.rangeStart+bindings.OpportunitiesIterator.currentRowIndexInRange+1>1}"></af:commandToolbarButton>
                    <af:image source="/afr/shuttleleftall_dis.png"
                              rendered="#{!(bindings.OpportunitiesIterator.rangeStart+bindings.OpportunitiesIterator.currentRowIndexInRange+1>1)}"
                              id="img0"/>
                    <af:commandToolbarButton actionListener="#{bindings.PreviousOpportunities.execute}"
                                             icon="/afr/shuttleleft_ena.png"
                                             shortDesc="#{nls['PREVIOUS_ROW']}"
                                             id="OppPrevious"
                                             partialSubmit="true"
                                             rendered="#{bindings.OpportunitiesIterator.rangeStart+bindings.OpportunitiesIterator.currentRowIndexInRange+1>1}"></af:commandToolbarButton>
                    <af:image source="/afr/shuttleleft_dis.png"
                              rendered="#{!(bindings.OpportunitiesIterator.rangeStart+bindings.OpportunitiesIterator.currentRowIndexInRange+1>1)}"
                              id="img1"/>
