            <af:inputText rows="10" columns="100"
                          value="#{bindings.OppDescription.inputValue}"
                          simple="true" maximumLength="4000"
                          id="OppDescriptionEditorText"/>
          </af:dialog>
        </af:popup>
        <af:popup id="OppSummaryPopup" contentDelivery="lazyUncached"
                  eventContext="launcher" childCreation="deferred">
          <af:dialog modal="true" resize="on" title="Text Editor" id="dialog1">
            <af:inputText rows="10" columns="100"
                          value="#{bindings.OppSummary.inputValue}"
                          simple="true" maximumLength="4000"
                          id="OppSummaryEditorText"/>
          </af:dialog>
        </af:popup>
        <af:popup id="sa4DeletePopup" contentDelivery="lazyUncached"
                  childCreation="deferred">
          <af:dialog type="yesNo" title="#{nls['WARNING']}"
                     dialogListener="#{ExecuteActionAfterConfirmation.processDialogEvent}"
                     modal="true" id="sa4DeleteDialog">
            <af:outputText value="#{nls['DELETE_WARNING_OPPORTUNITYPROCESSSTATU:#{pageFlowScope.OpportunityProcessStatusTable.currentRowAttributeValues.StatusCode}']} "
                           id="ot1"/>
            <f:attribute name="methodExpression"
                         value="#{'#{DeleteOpportunityProcessStatus.execute}'}"/>
          </af:dialog>
        </af:popup>
      </af:panelGroupLayout>
    </f:facet>
  </af:pageTemplate>
</jsp:root>



