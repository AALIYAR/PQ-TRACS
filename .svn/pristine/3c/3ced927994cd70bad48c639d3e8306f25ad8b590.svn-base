      <Attr Name="_DisplaySize" Value="200"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.pq.tracs.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
