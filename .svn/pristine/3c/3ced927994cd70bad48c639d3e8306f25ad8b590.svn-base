<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="SubCategoryValueLookup"
  Version="11.1.1.59.23"
  OrderBy="internal_value"
  BindingStyle="OracleName"
  CustomQuery="true"
  ComponentClass="com.pq.tracs.model.dao.TracsView"
  PageIterMode="Partial"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.pq.tracs.model.lookups.SubCategoryValueLookup_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="CAT_VALUE"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[select id,
value as internal_value, 
value as display_value 
from trax_category_sub_value 
where TCV_ID=(select id from trax_category_value where value= :CAT_VALUE and category_id = 4)]]>
  </SQLQuery>
  <ViewAttribute
    Name="Id"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="BIGINT"
    AliasName="ID"
    Expression="ID"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="22"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="InternalValue"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Precision="200"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="INTERNAL_VALUE"
    Expression="INTERNAL_VALUE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="200"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="DisplayValue"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Precision="200"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="DISPLAY_VALUE"
    Expression="DISPLAY_VALUE"
    SQLType="VARCHAR">
