                          simple="true" maximumLength="4000"
                          id="NTCsDescriptionEditorText"/>
          </af:dialog>
        </af:popup>
        <af:popup id="NTCsNotesPopup" contentDelivery="lazyUncached"
                  eventContext="launcher" childCreation="deferred">
          <af:dialog modal="true" resize="on" title="Text Editor" id="dialog1">
            <af:inputText rows="10" columns="100"
                          value="#{bindings.NTCsNotes.inputValue}" simple="true"
                          maximumLength="4000" id="NTCsNotesEditorText"/>
          </af:dialog>
        </af:popup>
        <af:popup id="jhsLovPopup" contentDelivery="lazyUncached"
                  eventContext="launcher"
                  binding="#{pageFlowScope.jhsLovPopup.component}">
          <af:dialog modal="true" title="#{pageFlowScope.LovItemBean.lovTitle}"
                     type="none" id="dialog2" resize="on"
                     stretchChildren="first"
                     contentWidth="#{pageFlowScope.LovItemBean.lovWidth}"
                     contentHeight="#{pageFlowScope.LovItemBean.lovHeight}">
            <af:region value="#{bindings.LovTaskFlow.regionModel}"
                       id="region0"/>
            <f:facet name="buttonBar">
              <af:group id="g2">
                <af:commandButton textAndAccessKey="#{nls['OK_BUTTON_LABEL']}"
                                  id="cb0" partialSubmit="true"
                                  actionListener="#{pageFlowScope.LovItemBean.handleLovSelection}"/>
                <af:commandButton textAndAccessKey="#{nls['CANCEL']}" id="cb1"
                                  partialSubmit="true"
                                  actionListener="#{pageFlowScope.LovItemBean.cancelLov}"/>
              </af:group>
            </f:facet>
          </af:dialog>
          <af:clientListener method="popupClosedListener" type="popupClosed"/>
          <af:serverListener type="serverPopupClosed"
                             method="#{pageFlowScope.LovItemBean.cancelLov}"/>
        </af:popup>
        <af:popup id="NTCsEmailTextPopup" contentDelivery="lazyUncached"
                  eventContext="launcher" childCreation="deferred">
          <af:dialog modal="true" resize="on" title="Text Editor" id="dialog3">
            <af:inputText rows="30" columns="100"
                          value="#{bindings.NTCsEmailText.inputValue}"
                          converter="oracle.genericDomain" simple="true"
                          id="NTCsEmailTextEditorText"/>
          </af:dialog>
        </af:popup>
        <af:popup id="NTCActivityAttachmentsDeletePopup"
                  contentDelivery="lazyUncached" childCreation="deferred">
          <af:dialog type="yesNo" title="#{nls['WARNING']}"
                     dialogListener="#{ExecuteActionAfterConfirmation.processDialogEvent}"
                     modal="true" id="NTCActivityAttachmentsDeleteDialog">
            <af:outputText value="#{nls['DELETE_WARNING_NTCACTIVITYATTACHMENT:#{pageFlowScope.NTCActivityAttachmentsTable.currentRowAttributeValues.AttachmentType}']} "
                           id="ot1"/>
            <f:attribute name="methodExpression"
                         value="#{'#{DeleteNTCActivityAttachments.execute}'}"/>
          </af:dialog>
        </af:popup>
      </af:panelGroupLayout>
    </f:facet>
  </af:pageTemplate>
</jsp:root>



