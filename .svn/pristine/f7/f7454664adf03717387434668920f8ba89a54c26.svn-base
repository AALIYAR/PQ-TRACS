   <SQLQuery><![CDATA[SELECT TitleContractGuarantee.ID, 
       TitleContractGuarantee.TITLE_ID, 
       TitleContractGuarantee.CONTRACT_GUARANTEE_ID, 
       Title.CATALOG_NUMBER, 
       Title.LONG_TITLE, 
       Title.PRODUCT_LINE,
       Title.PUBLICATION_START_YEAR, 
       Title.PUBLICATION_END_YEAR, 
       Title.FILM_AVAILABILITY, 
       Title.BIBLIOGRAPHIC_STATUS,
       Title.EXERCISING_RIGHTS, 
       Title.PRIMARY_PM_CLASS, 
       TitleContractGuarantee.CREATED_BY, 
       TitleContractGuarantee.CREATED_DATE, 
       TitleContractGuarantee.LAST_UPDATED_BY, 
       TitleContractGuarantee.LAST_UPDATED_DATE, 
       Title.CONTENT_OWNER_ID, 
       TraxContractTitle.STATUS
FROM TRAX_TITLE_CONTRACT_GUARANTEE TitleContractGuarantee
    INNER JOIN TRAX_TITLE Title
        LEFT OUTER JOIN TRAX_CONTENT_OWNER TraxContentOwner
            ON Title.CONTENT_OWNER_ID = TraxContentOwner.ID
        ON TitleContractGuarantee.TITLE_ID = Title.ID
    INNER JOIN TRAX_CONTRACT_GUARANTEE TraxContractGuarantee
        ON TitleContractGuarantee.CONTRACT_GUARANTEE_ID = TraxContractGuarantee.ID
    INNER JOIN TRAX_CONTRACT_TITLE TraxContractTitle
        ON TitleContractGuarantee.TITLE_ID = TraxContractTitle.TITLE_ID
        AND TraxContractGuarantee.CONTRACT_ID = TraxContractTitle.CONTRACT_ID]]></SQLQuery>
