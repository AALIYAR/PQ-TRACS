<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel"
                version="11.1.1.59.23" id="Tracs_UIShellPageDef"
                Package="com.pq.tracs.view.pageDefs"
                ControllerClass="oracle.jheadstart.controller.jsf.lifecycle.JhsPageLifecycle"
                SkipValidation="skipDataControls">
  <parameters/>
  <executables>
    <variableIterator id="variables"/>
    <page id="jhsPageTemplateBinding"
          path="com.pq.tracs.view.pageDefs.JhsPageTemplatePageDef"
          Refresh="ifNeeded"/>
  </executables>
  <bindings>
    <action id="Commit" RequiresUpdateModel="true" Action="commitTransaction"
            DataControl="TracsServiceDataControl"/>
    <action id="Rollback" RequiresUpdateModel="false"
            Action="rollbackTransaction" DataControl="TracsServiceDataControl"/>
  </bindings>
</pageDefinition>
