                       facetName="#{( bindings.CategoryIterator.currentRow!=null ) ? 'hasRows' : 'noRows'}">
            <f:facet name="noRows">
              <af:outputFormatted value="#{nls['NO_ROWS_FOUND']}"
                                  styleUsage="pageStamp"
                                  id="CategoryNoRowsFound"/>
            </f:facet>
            <f:facet name="hasRows">
              <!-- Pushed "disabled" on stretchContext, now: Stretching disabled on group level. -->
              <af:panelGroupLayout id="CategoryFC" layout="vertical">
                <f:facet name="separator">
                  <af:spacer width="10" height="10" id="sp2"/>
                </f:facet>
                <af:panelFormLayout rows="1" labelAlignment="top" maxColumns="3"
                                    id="CategoryFI">
                  <af:inputText id="CategoryName"
                                value="#{bindings.CategoryName.inputValue}"
                                label="#{bindings.CategoryName.hints.label}"
                                required="true"
                                rows="#{bindings.CategoryName.hints.displayHeight}"
                                columns="#{bindings.CategoryName.hints.displayWidth}"
                                maximumLength="40"
                                readOnly="#{((pageFlowScope.createModes.CreateCategory) and (!(jhsUserRoles['Category.create']))) or ((!pageFlowScope.createModes.CreateCategory) and (!(jhsUserRoles['Category.update'])))}">
                    <f:validator binding="#{bindings.CategoryName.validator}"/>
                  </af:inputText>
                  <af:selectOneChoice id="CategoryType"
                                      value="#{bindings.CategoryType.inputValue}"
                                      label="#{bindings.CategoryType.hints.label}"
                                      required="true"
                                      readOnly="#{((pageFlowScope.createModes.CreateCategory) and (!(jhsUserRoles['Category.create']))) or ((!pageFlowScope.createModes.CreateCategory) and (!(jhsUserRoles['Category.update'])))}"
                                      unselectedLabel="">
                    <af:selectItem id="si0" value="Lookup" label="Lookup"/>
                  </af:selectOneChoice>
                  <af:selectBooleanCheckbox id="CategorySingularFlag"
                                            value="#{bindings.CategorySingularFlag.inputValue}"
                                            label="#{bindings.CategorySingularFlag.hints.label}"
                                            readOnly="#{((pageFlowScope.createModes.CreateCategory) and (!(jhsUserRoles['Category.create']))) or ((!pageFlowScope.createModes.CreateCategory) and (!(jhsUserRoles['Category.update'])))}"></af:selectBooleanCheckbox>
                  <af:selectBooleanCheckbox id="CategoryActiveFlag"
                                            value="#{bindings.CategoryActiveFlag.inputValue}"
                                            label="#{bindings.CategoryActiveFlag.hints.label}"
                                            readOnly="#{((pageFlowScope.createModes.CreateCategory) and (!(jhsUserRoles['Category.create']))) or ((!pageFlowScope.createModes.CreateCategory) and (!(jhsUserRoles['Category.update'])))}"></af:selectBooleanCheckbox>
                  <af:selectBooleanCheckbox id="CategoryFullOrPartialFlag"
                                            value="#{bindings.CategoryFullOrPartialFlag.inputValue}"
                                            label="#{bindings.CategoryFullOrPartialFlag.hints.label}"
                                            readOnly="#{((pageFlowScope.createModes.CreateCategory) and (!(jhsUserRoles['Category.create']))) or ((!pageFlowScope.createModes.CreateCategory) and (!(jhsUserRoles['Category.update'])))}"></af:selectBooleanCheckbox>
                  <af:inputText id="CategoryCreatedBy"
                                value="#{bindings.CategoryCreatedBy.inputValue}"
                                label="#{bindings.CategoryCreatedBy.hints.label}"
                                required="#{bindings.CategoryCreatedBy.hints.mandatory}"
                                rows="#{bindings.CategoryCreatedBy.hints.displayHeight}"
                                columns="#{bindings.CategoryCreatedBy.hints.displayWidth}"
                                maximumLength="40" readOnly="true"
                                disabled="true">
                    <f:validator binding="#{bindings.CategoryCreatedBy.validator}"/>
                  </af:inputText>
                  <af:inputDate id="CategoryCreatedDate"
                                value="#{bindings.CategoryCreatedDate.inputValue}"
                                label="#{bindings.CategoryCreatedDate.hints.label}"
                                required="#{bindings.CategoryCreatedDate.hints.mandatory}"
                                readOnly="true" disabled="true">
                    <af:convertDateTime pattern="#{nls.datetimepattern}"/>
                  </af:inputDate>
                  <af:inputText id="CategoryLastUpdatedBy"
                                value="#{bindings.CategoryLastUpdatedBy.inputValue}"
                                label="#{bindings.CategoryLastUpdatedBy.hints.label}"
                                required="#{bindings.CategoryLastUpdatedBy.hints.mandatory}"
                                rows="#{bindings.CategoryLastUpdatedBy.hints.displayHeight}"
                                columns="#{bindings.CategoryLastUpdatedBy.hints.displayWidth}"
                                maximumLength="40" readOnly="true"
                                disabled="true">
                    <f:validator binding="#{bindings.CategoryLastUpdatedBy.validator}"/>
                  </af:inputText>
                  <af:inputDate id="CategoryLastUpdatedDate"
                                value="#{bindings.CategoryLastUpdatedDate.inputValue}"
                                label="#{bindings.CategoryLastUpdatedDate.hints.label}"
                                required="#{bindings.CategoryLastUpdatedDate.hints.mandatory}"
                                readOnly="true" disabled="true">
                    <af:convertDateTime pattern="#{nls.datetimepattern}"/>
                  </af:inputDate>
