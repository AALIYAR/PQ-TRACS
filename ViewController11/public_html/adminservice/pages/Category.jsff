                                         partialTriggers="">
                      <f:attribute name="JhsIgnoreChanges" value="true"/>
                      <af:panelGroupLayout id="qsAreaCategoryValues"
                                           rendered="#{pageFlowScope.searchCategoryValues.quickSearchMode}"
                                           layout="horizontal">
                        <af:selectOneChoice id="qsSelectCategoryValues"
                                            label="#{nls['FILTER_BY']}"
