                        <!-- Also create CreateCategoryValues bean so we can start table group in create mode as well -->
                        <af:commandImageLink id="CategoryValuesArl"
                                             immediate="false"
                                             rendered="#{ bindings.CategoryIterator.currentRow!=null  and (jhsUserRoles['CategoryValues.create'])}"
                                             partialSubmit="true"
                                             actionListener="#{pageFlowScope.CategoryValuesTable.addRow}"
                                             shortDesc="#{nls['ADD_ROW']}"
                                             icon="/jheadstart/images/addRow.png"/>
                        <af:commandToolbarButton text="Print"
                                                 id="CategoryValuesPrint"
                                                 shortDesc="Opens a new tab with printer friendly format.">
                          <af:showPrintablePageBehavior/>
                        </af:commandToolbarButton>
                      </af:toolbar>
                    </f:facet>
                    <f:facet name="statusbar">
                      <af:toolbar>
                        <af:outputText value="#{bindings.CategoryValuesIterator.estimatedRowCount} rows found"/>
                      </af:toolbar>
                    </f:facet>
                    <af:table id="CategoryValuesTab" contentDelivery="immediate"
