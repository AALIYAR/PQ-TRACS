                    <af:commandToolbarButton actionListener="#{bindings.FirstCategory.execute}"
                                             id="CategoryFirst"
                                             partialSubmit="true"
                                             icon="/afr/shuttleleftall_ena.png"
                                             shortDesc="#{nls['FIRST_ROW']}"
                                             rendered="#{bindings.CategoryIterator.rangeStart+bindings.CategoryIterator.currentRowIndexInRange+1>1}"></af:commandToolbarButton>
                    <af:image source="/afr/shuttleleftall_dis.png"
                              rendered="#{!(bindings.CategoryIterator.rangeStart+bindings.CategoryIterator.currentRowIndexInRange+1>1)}"
                              id="img0"/>
                    <af:commandToolbarButton actionListener="#{bindings.PreviousCategory.execute}"
                                             icon="/afr/shuttleleft_ena.png"
                                             shortDesc="#{nls['PREVIOUS_ROW']}"
                                             id="CategoryPrevious"
                                             partialSubmit="true"
                                             rendered="#{bindings.CategoryIterator.rangeStart+bindings.CategoryIterator.currentRowIndexInRange+1>1}"></af:commandToolbarButton>
                    <af:image source="/afr/shuttleleft_dis.png"
                              rendered="#{!(bindings.CategoryIterator.rangeStart+bindings.CategoryIterator.currentRowIndexInRange+1>1)}"
                              id="img1"/>
