                                        value=" [#{bindings.CategoryIterator.rangeStart+bindings.CategoryIterator.currentRowIndexInRange+1} / #{bindings.CategoryIterator.estimatedRowCount}] "
                                        id="of0"/>
                    <af:commandToolbarButton actionListener="#{bindings.NextCategory.execute}"
                                             icon="/afr/shuttleright_ena.png"
                                             shortDesc="#{nls['NEXT_ROW']}"
                                             id="CategoryNext"
                                             partialSubmit="true"
                                             rendered="#{bindings.CategoryIterator.rangeStart+bindings.CategoryIterator.currentRowIndexInRange+1 lt bindings.CategoryIterator.estimatedRowCount}"></af:commandToolbarButton>
                    <af:image source="/afr/shuttleright_dis.png"
                              rendered="#{!(bindings.CategoryIterator.rangeStart+bindings.CategoryIterator.currentRowIndexInRange+1 lt bindings.CategoryIterator.estimatedRowCount)}"
                              id="img2"/>
                    <af:commandToolbarButton actionListener="#{bindings.LastCategory.execute}"
                                             icon="/afr/shuttlerightall_ena.png"
                                             shortDesc="#{nls['LAST_ROW']}"
                                             id="CategoryLast"
                                             partialSubmit="true"
                                             rendered="#{bindings.CategoryIterator.rangeStart+bindings.CategoryIterator.currentRowIndexInRange+1 lt bindings.CategoryIterator.estimatedRowCount}"></af:commandToolbarButton>
                    <af:image source="/afr/shuttlerightall_dis.png"
                              rendered="#{!(bindings.CategoryIterator.rangeStart+bindings.CategoryIterator.currentRowIndexInRange+1 lt bindings.CategoryIterator.estimatedRowCount)}"
                              id="img3"/>
                  </af:panelGroupLayout>
                  <af:group id="gr0">
                    <af:commandToolbarButton actionListener="#{CreateCategory.execute}"
                                             action="CreateCategory"
                                             icon="/jheadstart/images/newRowInForm.png"
                                             partialSubmit="true"
                                             shortDesc="#{nls['NEW_BUTTON_LABEL_CATEGORY']}"
                                             rendered="#{!pageFlowScope.createModes.CreateCategory and (true) and (jhsUserRoles['Category.create'])}"
                                             id="CategoryNewButton"></af:commandToolbarButton>
                  </af:group>
                  <af:group id="gr1"></af:group>
                  <af:group id="gr2">
                    <af:commandToolbarButton text="Print" id="CategoryPrint"
                                             shortDesc="Opens a new tab with printer friendly format.">
                      <af:showPrintablePageBehavior/>
                    </af:commandToolbarButton>
                    <af:commandButton action="#{SaveCategory.execute}"
                                      rendered="#{(((((jhsUserRoles['Category.create']) or (jhsUserRoles['Category.update'])) or (jhsUserRoles['CategoryValues.create'])) or (jhsUserRoles['CategoryValues.update'])) or (jhsUserRoles['CategoryValues.delete'])) and (!pageFlowScope.hideSaveButton)}"
                                      textAndAccessKey="#{nls['SAVE_BUTTON_LABEL']}"
                                      id="CategorySaveButton0"
                                      partialSubmit="true"></af:commandButton>
                    <af:commandButton textAndAccessKey="#{nls['CANCEL']}"
                                      immediate="true" partialSubmit="true"
                                      rendered="#{!pageFlowScope.hideCancelButton}"
                                      actionListener="#{jhsRollback.execute}"
                                      action="Cancel"
                                      id="CategoryCancelButton0"></af:commandButton>
                  </af:group>
                </af:toolbar>
              </af:toolbox>
            </f:facet>
            <f:facet name="context"></f:facet>
          </af:panelHeader>
          <af:switcher rendered="#{bindings.CategoryIterator.findMode!='true'}"
