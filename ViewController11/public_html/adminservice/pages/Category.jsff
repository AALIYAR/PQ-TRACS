                          <af:switcher facetName="#{pageFlowScope.searchCategoryValues.searchItem}"
                                       id="qsAreaCategoryValuesSwitcher">
                            <f:facet name="CategoryValuesValue">
                              <af:inputText id="QuickSearchCategoryValuesValue"
                                            value="#{pageFlowScope.searchCategoryValues.searchText}"
                                            label="#{bindings.CategoryValuesTable.hints.Value.label}"
                                            simple="true"
                                            columns="#{bindings.CategoryValuesTable.hints.Value.displayWidth}"
                                            maximumLength="4000"></af:inputText>
                            </f:facet>
                            <f:facet name="CategoryValuesOrderBy">
                              <af:inputText id="QuickSearchCategoryValuesOrderBy"
                                            value="#{pageFlowScope.searchCategoryValues.searchText}"
                                            label="#{bindings.CategoryValuesTable.hints.OrderBy1.label}"
                                            simple="true"
                                            columns="#{bindings.CategoryValuesTable.hints.OrderBy1.displayWidth}"
                                            maximumLength="4000">
                                <af:convertNumber groupingUsed="false"
                                                  pattern="#{bindings.CategoryValuesTable.hints.OrderBy1.format}"/>
                              </af:inputText>
                            </f:facet>
                            <f:facet name="CategoryValuesDescription">
                              <af:inputText id="QuickSearchCategoryValuesDescription"
                                            value="#{pageFlowScope.searchCategoryValues.searchText}"
                                            label="#{bindings.CategoryValuesTable.hints.Description.label}"
                                            simple="true"
                                            columns="#{bindings.CategoryValuesTable.hints.Description.displayWidth}"
                                            maximumLength="4000"></af:inputText>
                            </f:facet>
                            <f:facet name="CategoryValuesActiveFlag">
                              <af:selectOneChoice id="QuickSearchCategoryValuesActiveFlag"
                                                  value="#{pageFlowScope.searchCategoryValues.searchText}"
                                                  label="#{bindings.CategoryValuesTable.hints.ActiveFlag.label}"
                                                  unselectedLabel=""
                                                  simple="true">
                                <af:selectItem id="si9" value="Y" label="Yes"/>
                                <af:selectItem id="si10" value="N" label="No"/>
                              </af:selectOneChoice>
                            </f:facet>
                            <f:facet name="CategoryValuesCreatedBy">
                              <af:inputText id="QuickSearchCategoryValuesCreatedBy"
                                            value="#{pageFlowScope.searchCategoryValues.searchText}"
                                            label="#{bindings.CategoryValuesTable.hints.CreatedBy.label}"
                                            simple="true"
                                            columns="#{bindings.CategoryValuesTable.hints.CreatedBy.displayWidth}"
                                            maximumLength="4000"></af:inputText>
                            </f:facet>
                            <f:facet name="CategoryValuesCreatedDate">
                              <af:inputDate id="QuickSearchCategoryValuesCreatedDate"
                                            value="#{pageFlowScope.searchCategoryValues.searchText}"
                                            label="#{bindings.CategoryValuesTable.hints.CreatedDate.label}"
                                            simple="true">
                                <af:convertDateTime pattern="#{nls.datepattern}"/>
                              </af:inputDate>
                            </f:facet>
                            <f:facet name="CategoryValuesLastUpdatedBy">
                              <af:inputText id="QuickSearchCategoryValuesLastUpdatedBy"
                                            value="#{pageFlowScope.searchCategoryValues.searchText}"
                                            label="#{bindings.CategoryValuesTable.hints.LastUpdatedBy.label}"
                                            simple="true"
                                            columns="#{bindings.CategoryValuesTable.hints.LastUpdatedBy.displayWidth}"
                                            maximumLength="4000"></af:inputText>
                            </f:facet>
                            <f:facet name="CategoryValuesLastUpdatedDate">
                              <af:inputDate id="QuickSearchCategoryValuesLastUpdatedDate"
                                            value="#{pageFlowScope.searchCategoryValues.searchText}"
                                            label="#{bindings.CategoryValuesTable.hints.LastUpdatedDate.label}"
                                            simple="true">
                                <af:convertDateTime pattern="#{nls.datepattern}"/>
                              </af:inputDate>
                            </f:facet>
                          </af:switcher>
                        </af:panelGroupLayout>
                        <af:spacer width="4" id="qsAreaCategoryValuesSpacer2"/>
                        <af:commandImageLink action="#{pageFlowScope.searchCategoryValues.quickSearch}"
                                             shortDesc="#{nls['FILTER_BY']}"
                                             icon="/jheadstart/images/go_ena.png"
                                             id="CategoryValuesQuickSearchButton"/>
                      </af:panelGroupLayout>
                    </af:panelGroupLayout>
                  </af:subform>
                </af:group>
                <af:panelGroupLayout layout="vertical" id="pgl0">
                  <af:panelCollection id="CategoryValuesPC" featuresOff=" "
                                      inlineStyle=" width:828px; ">
                    <f:facet name="menus">
                      <af:menu textAndAccessKey="#{nls['ACTION_MENU_LABEL']}"
                               id="menu0">
                        <af:menu textAndAccessKey="#{nls['GO_TO_MENU_LABEL']}"
                                 id="menu1">
                          <af:group id="CategoryValuestbb"
                                    rendered="#{ bindings.CategoryValuesIterator.currentRow!=null }">
                            <af:commandMenuItem actionListener="#{bindings.FirstCategoryValues.execute}"
                                                id="CategoryValuesFirst"
                                                partialSubmit="true"
                                                icon="/afr/shuttleleftall_ena.png"
                                                textAndAccessKey="#{nls['FIRST_ROW']}"></af:commandMenuItem>
                            <af:commandMenuItem actionListener="#{bindings.PreviousCategoryValues.execute}"
                                                icon="/afr/shuttleleft_ena.png"
                                                textAndAccessKey="#{nls['PREVIOUS_ROW']}"
                                                id="CategoryValuesPrevious"
                                                partialSubmit="true"></af:commandMenuItem>
                            <af:commandMenuItem actionListener="#{bindings.NextCategoryValues.execute}"
                                                icon="/afr/shuttleright_ena.png"
                                                textAndAccessKey="#{nls['NEXT_ROW']}"
                                                id="CategoryValuesNext"
                                                partialSubmit="true"></af:commandMenuItem>
                            <af:commandMenuItem actionListener="#{bindings.LastCategoryValues.execute}"
                                                icon="/afr/shuttlerightall_ena.png"
                                                textAndAccessKey="#{nls['LAST_ROW']}"
                                                id="CategoryValuesLast"
                                                partialSubmit="true"></af:commandMenuItem>
                          </af:group>
                        </af:menu>
