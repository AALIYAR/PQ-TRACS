                              fetchSize="#{bindings.CategoryValuesTable.rangeSize}"
                              emptyText="#{nls['NO_ROWS_FOUND']}"
                              binding="#{pageFlowScope.CategoryValuesTable.table}"
                              partialTriggers=" :::CategoryValuesPC ::CategoryValuesFirst ::CategoryValuesPrevious ::CategoryValuesNext ::CategoryValuesLast"
                              width="828px"
                              summary="#{nls['TABLE_TITLE_CATEGORYVALUES']}">
