                                            textAndAccessKey="#{nls['ADD_ROW']}"
                                            immediate="false"
                                            icon="/jheadstart/images/addRow.png"
                                            partialSubmit="true"
                                            actionListener="#{pageFlowScope.CategoryValuesTable.addRow}"
                                            rendered="#{ bindings.CategoryIterator.currentRow!=null  and (jhsUserRoles['CategoryValues.create'])}"></af:commandMenuItem>
                        <af:commandMenuItem id="CategoryValuesdelrmTM"
                                            textAndAccessKey="#{nls['DELETE_ROW']}"
                                            icon="/jheadstart/images/deleteRow.png"
                                            partialSubmit="true"
                                            immediate="true"
                                            rendered="#{jhsUserRoles['CategoryValues.delete']}">
                          <af:showPopupBehavior popupId="::CategoryValuesDeletePopup"/>
                        </af:commandMenuItem>
                      </af:menu>
                    </f:facet>
                    <f:facet name="toolbar">
