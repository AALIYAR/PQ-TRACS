                          <af:selectItem label="#{bindings.CategoryValuesTable.hints.Value.label}"
                                         value="CategoryValuesValue" id="si1"/>
                          <af:selectItem label="#{bindings.CategoryValuesTable.hints.OrderBy1.label}"
                                         value="CategoryValuesOrderBy"
                                         id="si2"/>
                          <af:selectItem label="#{bindings.CategoryValuesTable.hints.Description.label}"
                                         value="CategoryValuesDescription"
                                         id="si3"/>
                          <af:selectItem label="#{bindings.CategoryValuesTable.hints.ActiveFlag.label}"
                                         value="CategoryValuesActiveFlag"
                                         id="si4"/>
                          <af:selectItem label="#{bindings.CategoryValuesTable.hints.CreatedBy.label}"
                                         value="CategoryValuesCreatedBy"
                                         id="si5"/>
                          <af:selectItem label="#{bindings.CategoryValuesTable.hints.CreatedDate.label}"
                                         value="CategoryValuesCreatedDate"
                                         id="si6"/>
                          <af:selectItem label="#{bindings.CategoryValuesTable.hints.LastUpdatedBy.label}"
                                         value="CategoryValuesLastUpdatedBy"
                                         id="si7"/>
                          <af:selectItem label="#{bindings.CategoryValuesTable.hints.LastUpdatedDate.label}"
                                         value="CategoryValuesLastUpdatedDate"
                                         id="si8"/>
