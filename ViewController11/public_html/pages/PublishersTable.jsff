                        headerText="#{nls['ADVANCED_SEARCH']}" disclosed="true"
                        value="#{bindings.PublishersIteratorPublisherViewCriteriaAdvQuery.queryDescriptor}"
                        model="#{bindings.PublishersIteratorPublisherViewCriteriaAdvQuery.queryModel}"
                        queryListener="#{bindings.PublishersIteratorPublisherViewCriteriaAdvQuery.processQuery}"
                        queryOperationListener="#{bindings.PublishersIteratorPublisherViewCriteriaAdvQuery.processQueryOperation}"
                        rendered="#{!pageFlowScope.searchPublishers.quickSearchMode}"
                        rows="1" labelAlignment="top" maxColumns="4">
                <f:facet name="toolbar">
                  <af:commandButton partialSubmit="true" id="qsButtonPub"
                                    immediate="true"
                                    textAndAccessKey="#{nls['QUICK_SEARCH']}"
                                    action="#{pageFlowScope.searchPublishers.switchToQuickSearchMode}"/>
                </f:facet>
              </af:query>
              <af:quickQuery id="qsAreaPubSearchOptions"
                             label="#{nls['FILTER_BY']}"
                             value="#{bindings.PublishersIteratorPublisherViewCriteriaAdvQuery.queryDescriptor}"
                             model="#{bindings.PublishersIteratorPublisherViewCriteriaAdvQuery.queryModel}"
                             queryListener="#{bindings.PublishersIteratorPublisherViewCriteriaAdvQuery.processQuery}"
                             rendered="#{pageFlowScope.searchPublishers.quickSearchMode}">
                <f:facet name="end">
                  <af:commandLink id="asButtonPub"
                                  textAndAccessKey="#{nls['ADVANCED_SEARCH']}"
                                  partialSubmit="true" immediate="true"
                                  action="#{pageFlowScope.searchPublishers.switchToAdvancedSearchMode}"/>
                </f:facet>
              </af:quickQuery>
            </af:panelGroupLayout>
          </af:panelHeader>
          <af:panelGroupLayout layout="vertical" id="pgl0">
            <af:panelCollection id="PubPC" featuresOff=" "
                                styleClass="AFStretchWidth">
