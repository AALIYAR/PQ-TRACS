                        contextMenuSelect="true" rowSelection="single"
                        selectionListener="#{pageFlowScope.PublishersTable.makeCurrent}"
                        selectedRowKeys="#{pageFlowScope.PublishersTable.selectedRow}"
                        rowDisclosureListener="#{pageFlowScope.PublishersTable.discloseRow}"
