                    <af:commandButton actionListener="#{CreatePublishers.execute}"
                                      action="CreatePub"
                                      textAndAccessKey="#{nls['NEW_BUTTON_LABEL_PUBLISHERS']}"
                                      rendered="#{(true) and (jhsUserRoles['RM, ADMIN, AllButTitl, AllButAdmn'])}"
                                      id="PubNewButton"></af:commandButton>
                  </af:group>
                  <af:group id="gr1"></af:group>
                  <af:group id="gr2">
