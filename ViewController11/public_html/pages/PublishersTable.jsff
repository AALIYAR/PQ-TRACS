                        <af:commandMenuItem id="mi1"
                                            textAndAccessKey="#{nls['DETAIL_VIEW_BUTTON_LABEL']}"
                                            action="PublishersDetails"></af:commandMenuItem>
                      </af:menu>
                      <af:commandMenuItem id="Pubdelrm"
