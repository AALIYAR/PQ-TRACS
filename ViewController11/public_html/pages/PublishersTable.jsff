                                      immediate="false"
                                      icon="/jheadstart/images/addRow.png"
                                      partialSubmit="true"
                                      actionListener="#{pageFlowScope.PublishersTable.addRow}"
                                      rendered="#{(true) and (jhsUserRoles['RM, ADMIN, AllButTitl, AllButAdmn'])}"></af:commandMenuItem>
                  <af:commandMenuItem id="PubdelrmTM"
                                      textAndAccessKey="#{nls['DELETE_ROW']}"
                                      icon="/jheadstart/images/deleteRow.png"
                                      partialSubmit="true" immediate="true"
                                      rendered="#{jhsUserRoles['ADMIN']}">
                    <af:showPopupBehavior popupId="::PubDeletePopup"/>
                  </af:commandMenuItem>
                  <af:commandMenuItem partialSubmit="true"
                                      textAndAccessKey="#{nls['EXPORT']}"
                                      icon="/jheadstart/images/excel.png"
                                      immediate="true" id="cmi0">
                    <af:exportCollectionActionListener exportedId="PubTab"
                                                       type="excelHTML"
                                                       filename="Publishers.xls"/>
                  </af:commandMenuItem>
                </af:menu>
              </f:facet>
              <f:facet name="toolbar">
                <af:toolbar id="tbar1">
                  <!-- Also create CreatePublishers bean so we can start table group in create mode as well -->
                  <af:commandImageLink id="PubArl" immediate="false"
                                       rendered="#{(true) and (jhsUserRoles['RM, ADMIN, AllButTitl, AllButAdmn'])}"
                                       partialSubmit="true"
                                       actionListener="#{pageFlowScope.PublishersTable.addRow}"
                                       shortDesc="#{nls['ADD_ROW']}"
                                       icon="/jheadstart/images/addRow.png"/>
                  <af:commandImageLink immediate="true" partialSubmit="true"
                                       id="cl0" shortDesc="#{nls['EXPORT']}"
                                       icon="/jheadstart/images/excel.png">
                    <af:exportCollectionActionListener exportedId="PubTab"
