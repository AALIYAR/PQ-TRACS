<?xml version='1.0' encoding="UTF-8"?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1"
          xmlns:af="http://xmlns.oracle.com/adf/faces/rich">
  <af:panelHeader text="An unexpected error occurred, please contact your system administrator"
                  id="ph1">
    <af:panelFormLayout id="pfl1">
      <af:inputText label="Error Message"
                    value="#{exceptionHandlerBean.errorMessage}" readOnly="true"
                    id="it1"/>
      <af:inputText label="Stack Trace"
                    value="#{exceptionHandlerBean.stackTrace}" rows="30"
                    columns="160" readOnly="true" id="it2"/>
    </af:panelFormLayout>
  </af:panelHeader>
</jsp:root>
  