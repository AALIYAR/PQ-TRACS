                      <af:commandMenuItem actionListener="#{bindings.LastUnifiedTitleSearch.execute}"
                                          icon="/afr/shuttlerightall_ena.png"
                                          textAndAccessKey="#{nls['LAST_ROW']}"
                                          id="UTSLast" partialSubmit="true"></af:commandMenuItem>
