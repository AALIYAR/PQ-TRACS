                                <af:popup id="NTCPublisherTitlesTablePopup"
                                          childCreation="deferred">
                                  <af:menu id="menu2">
                                    <af:menu text="#{nls['GO_TO_MENU_LABEL']}"
                                             id="menu3"></af:menu>
                                    <af:commandMenuItem id="NTCPublisherTitlesdelrm"
                                                        textAndAccessKey="#{nls['DELETE_ROW']}"
                                                        icon="/jheadstart/images/deleteRow.png"
                                                        partialSubmit="true"
                                                        immediate="true">
                                      <af:showPopupBehavior popupId=":::NTCPublisherTitlesDeletePopup"/>
                                    </af:commandMenuItem>
                                  </af:menu>
                                </af:popup>
                              </f:facet>
                              <af:column id="NTCPublisherTitlesActionColumn"
                                         width="20" align="center"
                                         headerText=" ">
                                <af:panelGroupLayout layout="horizontal"
                                                     id="pgl3">
                                  <af:commandImageLink id="NTCPublisherTitlesdelrl"
                                                       partialSubmit="true"
                                                       immediate="true"
                                                       shortDesc="#{nls['DELETE_ROW']}"
                                                       icon="/jheadstart/images/deleteRow.png">
                                    <af:showPopupBehavior popupId=":::NTCPublisherTitlesDeletePopup"/>
                                  </af:commandImageLink>
                                </af:panelGroupLayout>
                              </af:column>
                              <af:column id="NTCPublisherTitlesTargetTrackingNoCol"
