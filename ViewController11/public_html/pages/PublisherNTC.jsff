<?xml version='1.0' encoding="UTF-8"?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1"
          xmlns:h="http://java.sun.com/jsf/html"
          xmlns:f="http://java.sun.com/jsf/core"
          xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
          xmlns:trh="http://myfaces.apache.org/trinidad/html"
          xmlns:tr="http://myfaces.apache.org/trinidad"
          xmlns:dvt="http://xmlns.oracle.com/dss/adf/faces">
  <jsp:directive.page contentType="text/html;charset=UTF-8"/>
  <af:pageTemplate id="pt"
                   viewId="/common/pageTemplates/JhsRegionTemplateNoStretch.jspx">
    <f:facet name="pageContent">
      <!-- Pushed "disabled" on stretchContext, now: Stretching disabled on group level. -->
      <af:panelGroupLayout id="pcpg" layout="scroll">
        <af:panelGroupLayout layout="vertical" id="PublisherNTCTopLc"
                             partialTriggers=" PublisherNTCfbb PublisherNTCNewButton PublisherNTCSaveButton0 PublisherNTCCancelButton0 PublisherNTCSaveButton1 PublisherNTCCancelButton1"
                             rendered="#{jhsUserRoles['AM, PM, AST, GCA, RO, RM, CO, ADMIN, USER, ReadOnly, ActOnly, AllButTitl, AllButAdmn, OVR, RORoyalty']}">
          <f:facet name="separator">
            <af:spacer width="10" height="10" id="sp0"/>
          </f:facet>
          <!-- Pushed "disabled" on stretchContext, now: Stretching disabled on group level. -->
          <af:panelHeader id="ph0"
                          text="#{pageFlowScope.createModes.CreatePublisherNTC ? nls['INSERT_TITLE_PUBLISHERNTC'] :  nls['EDIT_TITLE_PUBLISHERNT: #{bindings.PublisherNTCOldPublisher.inputValue}'] }">
            <f:facet name="toolbar">
              <af:toolbox id="tbox0">
                <af:toolbar id="tbar0">
                  <af:panelGroupLayout id="PublisherNTCfbb" layout="horizontal"
                                       rendered="#{ bindings.PublisherNTCIterator.currentRow!=null  and bindings.PublisherNTCIterator.estimatedRowCount>1 and !pageFlowScope.createModes.CreatePublisherNTC and !pageFlowScope.hideFormBrowseButtons}"
                                       partialTriggers="PublisherNTCFirst PublisherNTCPrevious PublisherNTCNext PublisherNTCLast">
                    <af:commandToolbarButton actionListener="#{bindings.FirstPublisherNTC.execute}"
                                             id="PublisherNTCFirst"
                                             partialSubmit="true"
                                             icon="/afr/shuttleleftall_ena.png"
                                             shortDesc="#{nls['FIRST_ROW']}"
                                             rendered="#{bindings.PublisherNTCIterator.rangeStart+bindings.PublisherNTCIterator.currentRowIndexInRange+1>1}"></af:commandToolbarButton>
                    <af:image source="/afr/shuttleleftall_dis.png"
                              rendered="#{!(bindings.PublisherNTCIterator.rangeStart+bindings.PublisherNTCIterator.currentRowIndexInRange+1>1)}"
                              id="img0"/>
                    <af:commandToolbarButton actionListener="#{bindings.PreviousPublisherNTC.execute}"
                                             icon="/afr/shuttleleft_ena.png"
                                             shortDesc="#{nls['PREVIOUS_ROW']}"
                                             id="PublisherNTCPrevious"
                                             partialSubmit="true"
                                             rendered="#{bindings.PublisherNTCIterator.rangeStart+bindings.PublisherNTCIterator.currentRowIndexInRange+1>1}"></af:commandToolbarButton>
                    <af:image source="/afr/shuttleleft_dis.png"
                              rendered="#{!(bindings.PublisherNTCIterator.rangeStart+bindings.PublisherNTCIterator.currentRowIndexInRange+1>1)}"
                              id="img1"/>
                    <af:spacer width="4" height="1" id="sp1"/>
                    <af:outputFormatted styleUsage="instruction"
                                        inlineStyle="white-space:nowrap; vertical-align:bottom;"
                                        value=" [#{bindings.PublisherNTCIterator.rangeStart+bindings.PublisherNTCIterator.currentRowIndexInRange+1} / #{bindings.PublisherNTCIterator.estimatedRowCount}] "
                                        id="of0"/>
                    <af:commandToolbarButton actionListener="#{bindings.NextPublisherNTC.execute}"
                                             icon="/afr/shuttleright_ena.png"
                                             shortDesc="#{nls['NEXT_ROW']}"
                                             id="PublisherNTCNext"
                                             partialSubmit="true"
                                             rendered="#{bindings.PublisherNTCIterator.rangeStart+bindings.PublisherNTCIterator.currentRowIndexInRange+1 lt bindings.PublisherNTCIterator.estimatedRowCount}"></af:commandToolbarButton>
                    <af:image source="/afr/shuttleright_dis.png"
                              rendered="#{!(bindings.PublisherNTCIterator.rangeStart+bindings.PublisherNTCIterator.currentRowIndexInRange+1 lt bindings.PublisherNTCIterator.estimatedRowCount)}"
                              id="img2"/>
                    <af:commandToolbarButton actionListener="#{bindings.LastPublisherNTC.execute}"
                                             icon="/afr/shuttlerightall_ena.png"
                                             shortDesc="#{nls['LAST_ROW']}"
                                             id="PublisherNTCLast"
                                             partialSubmit="true"
                                             rendered="#{bindings.PublisherNTCIterator.rangeStart+bindings.PublisherNTCIterator.currentRowIndexInRange+1 lt bindings.PublisherNTCIterator.estimatedRowCount}"></af:commandToolbarButton>
                    <af:image source="/afr/shuttlerightall_dis.png"
                              rendered="#{!(bindings.PublisherNTCIterator.rangeStart+bindings.PublisherNTCIterator.currentRowIndexInRange+1 lt bindings.PublisherNTCIterator.estimatedRowCount)}"
                              id="img3"/>
                  </af:panelGroupLayout>
                  <af:group id="gr0">
                    <af:commandToolbarButton actionListener="#{CreatePublisherNTC.execute}"
                                             action="CreatePublisherNTC"
                                             icon="/jheadstart/images/newRowInForm.png"
                                             partialSubmit="true"
                                             shortDesc="#{nls['NEW_BUTTON_LABEL_PUBLISHERNTC']}"
                                             rendered="#{!pageFlowScope.createModes.CreatePublisherNTC and (true) }"
                                             id="PublisherNTCNewButton"></af:commandToolbarButton>
                  </af:group>
                  <af:group id="gr1"></af:group>
                  <af:group id="gr2">
                    <af:commandToolbarButton text="Print" id="PublisherNTCPrint"
                                             shortDesc="Opens a new tab with printer friendly format.">
                      <af:showPrintablePageBehavior/>
                    </af:commandToolbarButton>
                    <af:commandButton action="#{SavePublisherNTC.execute}"
                                      rendered="#{!pageFlowScope.hideSaveButton}"
                                      textAndAccessKey="#{nls['SAVE_BUTTON_LABEL']}"
                                      id="PublisherNTCSaveButton0"
                                      partialSubmit="true"></af:commandButton>
                    <af:commandButton textAndAccessKey="#{nls['CANCEL']}"
                                      immediate="true" partialSubmit="true"
                                      rendered="#{!pageFlowScope.hideCancelButton}"
                                      actionListener="#{jhsRollback.execute}"
                                      action="Cancel"
                                      id="PublisherNTCCancelButton0"></af:commandButton>
                  </af:group>
                </af:toolbar>
              </af:toolbox>
            </f:facet>
            <f:facet name="context"></f:facet>
          </af:panelHeader>
          <af:switcher rendered="#{bindings.PublisherNTCIterator.findMode!='true'}"
                       id="PublisherNTCSw" defaultFacet="hasRows"
                       facetName="#{( bindings.PublisherNTCIterator.currentRow!=null ) ? 'hasRows' : 'noRows'}">
            <f:facet name="noRows">
              <af:outputFormatted value="#{nls['NO_ROWS_FOUND']}"
                                  styleUsage="pageStamp"
                                  id="PublisherNTCNoRowsFound"/>
            </f:facet>
            <f:facet name="hasRows">
              <!-- Pushed "disabled" on stretchContext, now: Stretching disabled on group level. -->
              <af:panelGroupLayout id="PublisherNTCFC" layout="vertical">
                <f:facet name="separator">
                  <af:spacer width="10" height="10" id="sp2"/>
                </f:facet>
                <af:panelFormLayout rows="1" labelAlignment="top" maxColumns="3"
                                    id="PublisherNTCFI">
                  <af:inputText id="PublisherNTCNtcNumber"
                                value="#{bindings.PublisherNTCNtcNumber.inputValue}"
                                label="#{bindings.PublisherNTCNtcNumber.hints.label}"
                                required="#{bindings.PublisherNTCNtcNumber.hints.mandatory}"
                                rows="#{bindings.PublisherNTCNtcNumber.hints.displayHeight}"
                                columns="#{bindings.PublisherNTCNtcNumber.hints.displayWidth}"
                                maximumLength="38" readOnly="true"
                                shortDesc="#{bindings.PublisherNTCNtcNumber.hints.tooltip}">
                    <f:validator binding="#{bindings.PublisherNTCNtcNumber.validator}"/>
                    <af:convertNumber groupingUsed="false"
                                      pattern="#{bindings.PublisherNTCNtcNumber.format}"/>
                  </af:inputText>
                  <af:selectOneChoice id="PublisherNTCSubType"
                                      value="#{bindings.PublisherNTCSubType.inputValue}"
                                      label="#{bindings.PublisherNTCSubType.hints.label}"
                                      required="#{bindings.PublisherNTCSubType.hints.mandatory}"
                                      shortDesc="#{bindings.PublisherNTCSubType.hints.tooltip}">
                    <f:selectItems value="#{bindings.PublisherNTCSubType.items}"/>
                  </af:selectOneChoice>
                  <af:inputListOfValues id="PublisherNTCOldPublisher"
                                        popupTitle="#{nls['SEARCH_AND_SELECT']} #{bindings.PublisherNTCOldPublisher.hints.label}"
                                        searchDesc="#{nls['SEARCH_AND_SELECT']} #{bindings.PublisherNTCOldPublisher.hints.label}"
                                        value="#{bindings.PublisherNTCOldPublisher.inputValue}"
                                        label="#{bindings.PublisherNTCOldPublisher.hints.label}"
                                        required="#{bindings.PublisherNTCOldPublisher.hints.mandatory}"
                                        columns="#{bindings.PublisherNTCOldPublisher.hints.displayWidth}"
                                        maximumLength="400"
                                        shortDesc="#{bindings.PublisherNTCOldPublisher.hints.tooltip}"
                                        autoSubmit="true"
                                        model="#{bindings.PublisherNTCOldPublisher.listOfValuesModel}">
                    <f:validator binding="#{bindings.PublisherNTCOldPublisher.validator}"/>
                  </af:inputListOfValues>
                  <af:inputText id="PublisherNTCNewPublisher"
                                value="#{bindings.PublisherNTCNewPublisher.inputValue}"
                                label="#{bindings.PublisherNTCNewPublisher.hints.label}"
                                required="#{bindings.PublisherNTCNewPublisher.hints.mandatory}"
                                rows="#{bindings.PublisherNTCNewPublisher.hints.displayHeight}"
                                columns="#{bindings.PublisherNTCNewPublisher.hints.displayWidth}"
                                maximumLength="400"
                                shortDesc="#{bindings.PublisherNTCNewPublisher.hints.tooltip}">
                    <f:validator binding="#{bindings.PublisherNTCNewPublisher.validator}"/>
                  </af:inputText>
                  <af:inputText id="PublisherNTCContact"
                                value="#{bindings.PublisherNTCContact.inputValue}"
                                label="#{bindings.PublisherNTCContact.hints.label}"
                                required="#{bindings.PublisherNTCContact.hints.mandatory}"
                                rows="#{bindings.PublisherNTCContact.hints.displayHeight}"
                                columns="#{bindings.PublisherNTCContact.hints.displayWidth}"
                                maximumLength="100"
                                shortDesc="#{bindings.PublisherNTCContact.hints.tooltip}">
                    <f:validator binding="#{bindings.PublisherNTCContact.validator}"/>
                  </af:inputText>
                  <af:inputText id="PublisherNTCAddress"
                                value="#{bindings.PublisherNTCAddress.inputValue}"
                                label="#{bindings.PublisherNTCAddress.hints.label}"
                                required="#{bindings.PublisherNTCAddress.hints.mandatory}"
                                rows="#{bindings.PublisherNTCAddress.hints.displayHeight}"
                                columns="#{bindings.PublisherNTCAddress.hints.displayWidth}"
                                maximumLength="200"
                                shortDesc="#{bindings.PublisherNTCAddress.hints.tooltip}">
                    <f:validator binding="#{bindings.PublisherNTCAddress.validator}"/>
                  </af:inputText>
                  <af:inputText id="PublisherNTCPhoneNumber"
                                value="#{bindings.PublisherNTCPhoneNumber.inputValue}"
                                label="#{bindings.PublisherNTCPhoneNumber.hints.label}"
                                required="#{bindings.PublisherNTCPhoneNumber.hints.mandatory}"
                                rows="#{bindings.PublisherNTCPhoneNumber.hints.displayHeight}"
                                columns="#{bindings.PublisherNTCPhoneNumber.hints.displayWidth}"
                                maximumLength="50"
                                shortDesc="#{bindings.PublisherNTCPhoneNumber.hints.tooltip}">
                    <f:validator binding="#{bindings.PublisherNTCPhoneNumber.validator}"/>
                  </af:inputText>
                  <af:inputText id="PublisherNTCEmail"
                                value="#{bindings.PublisherNTCEmail.inputValue}"
                                label="#{bindings.PublisherNTCEmail.hints.label}"
                                required="#{bindings.PublisherNTCEmail.hints.mandatory}"
                                rows="#{bindings.PublisherNTCEmail.hints.displayHeight}"
                                columns="#{bindings.PublisherNTCEmail.hints.displayWidth}"
                                maximumLength="100"
                                shortDesc="#{bindings.PublisherNTCEmail.hints.tooltip}">
                    <f:validator binding="#{bindings.PublisherNTCEmail.validator}"/>
                  </af:inputText>
                  <af:inputDate id="PublisherNTCDateOfSale"
                                value="#{bindings.PublisherNTCDateOfSale.inputValue}"
                                label="#{bindings.PublisherNTCDateOfSale.hints.label}"
                                required="#{bindings.PublisherNTCDateOfSale.hints.mandatory}"
                                shortDesc="#{bindings.PublisherNTCDateOfSale.hints.tooltip}">
                    <af:convertDateTime pattern="#{nls.datetimepattern}"/>
                  </af:inputDate>
                  <af:inputText id="PublisherNTCDescription"
                                value="#{bindings.PublisherNTCDescription.inputValue}"
                                label="#{bindings.PublisherNTCDescription.hints.label}"
                                required="#{bindings.PublisherNTCDescription.hints.mandatory}"
                                rows="#{bindings.PublisherNTCDescription.hints.displayHeight}"
                                columns="#{bindings.PublisherNTCDescription.hints.displayWidth}"
                                maximumLength="4000"
                                shortDesc="#{bindings.PublisherNTCDescription.hints.tooltip}">
                    <f:validator binding="#{bindings.PublisherNTCDescription.validator}"/>
                  </af:inputText>
                  <af:panelLabelAndMessage label="#{bindings.PublisherNTCNotes.hints.label}"
                                           shortDesc="#{bindings.PublisherNTCNotes.hints.tooltip}"
                                           showRequired="#{bindings.PublisherNTCNotes.hints.mandatory}"
                                           id="PublisherNTCNotesLabel">
                    <af:inputText id="PublisherNTCNotes"
                                  value="#{bindings.PublisherNTCNotes.inputValue}"
                                  required="#{bindings.PublisherNTCNotes.hints.mandatory}"
                                  rows="#{bindings.PublisherNTCNotes.hints.displayHeight}"
                                  columns="#{bindings.PublisherNTCNotes.hints.displayWidth}"
                                  maximumLength="4000" autoSubmit="true"
                                  simple="true"
                                  partialTriggers="PublisherNTCNotesEditorLink PublisherNTCNotesEditorText">
                      <f:validator binding="#{bindings.PublisherNTCNotes.validator}"/>
                    </af:inputText>
                    <f:facet name="end">
                      <af:panelGroupLayout id="pgl0" valign="top"
                                           layout="horizontal">
                        <af:commandLink id="PublisherNTCNotesEditorLink"
                                        immediate="true" partialSubmit="true">
                          <af:showPopupBehavior popupId="PublisherNTCNotesPopup"/>
                          <af:image source="/jheadstart/images/editor.gif"
                                    id="PublisherNTCNotesIcon"
                                    clientComponent="true"/>
                        </af:commandLink>
                      </af:panelGroupLayout>
                    </f:facet>
                  </af:panelLabelAndMessage>
                </af:panelFormLayout>
                <af:panelHeader id="ph1" text=" ">
                  <f:facet name="toolbar">
                    <af:toolbox id="toolbox0">
                      <af:toolbar id="toolbar0">
                        <af:group id="gr3">
                          <af:commandButton action="#{SavePublisherNTC.execute}"
                                            rendered="#{!pageFlowScope.hideSaveButton}"
                                            textAndAccessKey="#{nls['SAVE_BUTTON_LABEL']}"
                                            id="PublisherNTCSaveButton1"
                                            partialSubmit="true"></af:commandButton>
                          <af:commandButton textAndAccessKey="#{nls['CANCEL']}"
                                            immediate="true"
                                            partialSubmit="true"
                                            rendered="#{!pageFlowScope.hideCancelButton}"
                                            actionListener="#{jhsRollback.execute}"
                                            action="Cancel"
                                            id="PublisherNTCCancelButton1"></af:commandButton>
                        </af:group>
                      </af:toolbar>
                    </af:toolbox>
                  </f:facet>
                </af:panelHeader>
                <af:panelTabbed position="above" childCreation="lazy"
                                dimensionsFrom="auto" inlineStyle="    "
                                id="PublisherNTCRegionsTabs">
                  <af:showDetailItem stretchChildren="first"
                                     textAndAccessKey="Titles"
                                     rendered="#{true and jhsUserRoles['AM, PM, AST, GCA, RO, RM, CO, ADMIN, USER, ReadOnly, ActOnly, AllButTitl, AllButAdmn, OVR, RORoyalty']}"
                                     disabled="#{!(true and jhsUserRoles['AM, PM, AST, GCA, RO, RM, CO, ADMIN, USER, ReadOnly, ActOnly, AllButTitl, AllButAdmn, OVR, RORoyalty'])}"
                                     id="PublisherNTCRegionsNTCPublisherTitlesTab">
                    <af:panelGroupLayout id="pgl1" layout="scroll">
                      <af:panelGroupLayout layout="vertical"
                                           id="NTCPublisherTitlesTopLc"
                                           partialTriggers=" PublisherNTCTopLc NTCPublisherTitlesDeleteDialog NTCPublisherTitlesPC:NTCPublisherTitlesArm NTCPublisherTitlesPC:NTCPublisherTitlesdelrmTM NTCPublisherTitlesPC:NTCPublisherTitlesArl NTCPublisherTitlesPC:NTCPublisherTitlesTab:NTCPublisherTitlesdelrm NTCPublisherTitlesPC:NTCPublisherTitlesTab:NTCPublisherTitlesdelrl"
                                           rendered="#{jhsUserRoles['AM, PM, AST, GCA, RO, RM, CO, ADMIN, USER, ReadOnly, ActOnly, AllButTitl, AllButAdmn, OVR, RORoyalty']}">
                        <f:facet name="separator">
                          <af:spacer width="10" height="10" id="sp3"/>
                        </f:facet>
                        <!-- Pushed "disabled" on stretchContext, now: Stretching disabled on group level. -->
                        <af:panelHeader id="ph2"
                                        text="#{nls['TABLE_TITLE_NTCPUBLISHERTITLES']}">
                          <f:facet name="toolbar">
                            <af:toolbox id="tbox1">
                              <af:toolbar id="tbar1">
                                <af:group id="gr4"></af:group>
                                <af:group id="gr5"></af:group>
                              </af:toolbar>
                            </af:toolbox>
                          </f:facet>
                          <f:facet name="context"></f:facet>
                        </af:panelHeader>
                        <af:panelGroupLayout layout="vertical" id="pgl2">
                          <af:panelCollection id="NTCPublisherTitlesPC"
                                              featuresOff=" "
                                              inlineStyle=" width:892px; ">
                            <f:facet name="menus">
                              <af:menu textAndAccessKey="#{nls['ACTION_MENU_LABEL']}"
                                       id="menu0">
                                <af:menu textAndAccessKey="#{nls['GO_TO_MENU_LABEL']}"
                                         id="menu1">
                                  <af:group id="NTCPublisherTitlestbb"
                                            rendered="#{ bindings.NTCPublisherTitlesIterator.currentRow!=null }">
                                    <af:commandMenuItem actionListener="#{bindings.FirstNTCPublisherTitles.execute}"
                                                        id="NTCPublisherTitlesFirst"
                                                        partialSubmit="true"
                                                        icon="/afr/shuttleleftall_ena.png"
                                                        textAndAccessKey="#{nls['FIRST_ROW']}"></af:commandMenuItem>
                                    <af:commandMenuItem actionListener="#{bindings.PreviousNTCPublisherTitles.execute}"
                                                        icon="/afr/shuttleleft_ena.png"
                                                        textAndAccessKey="#{nls['PREVIOUS_ROW']}"
                                                        id="NTCPublisherTitlesPrevious"
                                                        partialSubmit="true"></af:commandMenuItem>
                                    <af:commandMenuItem actionListener="#{bindings.NextNTCPublisherTitles.execute}"
                                                        icon="/afr/shuttleright_ena.png"
                                                        textAndAccessKey="#{nls['NEXT_ROW']}"
                                                        id="NTCPublisherTitlesNext"
                                                        partialSubmit="true"></af:commandMenuItem>
                                    <af:commandMenuItem actionListener="#{bindings.LastNTCPublisherTitles.execute}"
                                                        icon="/afr/shuttlerightall_ena.png"
                                                        textAndAccessKey="#{nls['LAST_ROW']}"
                                                        id="NTCPublisherTitlesLast"
                                                        partialSubmit="true"></af:commandMenuItem>
                                  </af:group>
                                </af:menu>
                                <af:commandMenuItem id="NTCPublisherTitlesArm"
                                                    textAndAccessKey="#{nls['ADD_ROW']}"
                                                    immediate="false"
                                                    icon="/jheadstart/images/addRow.png"
                                                    partialSubmit="true"
                                                    actionListener="#{pageFlowScope.NTCPublisherTitlesTable.addRow}"
                                                    rendered="#{ bindings.PublisherNTCIterator.currentRow!=null  }"></af:commandMenuItem>
                                <af:commandMenuItem id="NTCPublisherTitlesdelrmTM"
                                                    textAndAccessKey="#{nls['DELETE_ROW']}"
                                                    icon="/jheadstart/images/deleteRow.png"
                                                    partialSubmit="true"
                                                    immediate="true">
                                  <af:showPopupBehavior popupId="::NTCPublisherTitlesDeletePopup"/>
                                </af:commandMenuItem>
                              </af:menu>
                            </f:facet>
                            <f:facet name="toolbar">
                              <af:toolbar id="tbar2">
                                <!-- Also create CreateNTCPublisherTitles bean so we can start table group in create mode as well -->
                                <af:commandImageLink id="NTCPublisherTitlesArl"
                                                     immediate="false"
                                                     rendered="#{ bindings.PublisherNTCIterator.currentRow!=null  }"
                                                     partialSubmit="true"
                                                     actionListener="#{pageFlowScope.NTCPublisherTitlesTable.addRow}"
                                                     shortDesc="#{nls['ADD_ROW']}"
                                                     icon="/jheadstart/images/addRow.png"/>
                                <af:commandToolbarButton text="Print"
                                                         id="NTCPublisherTitlesPrint"
                                                         shortDesc="Opens a new tab with printer friendly format.">
                                  <af:showPrintablePageBehavior/>
                                </af:commandToolbarButton>
                              </af:toolbar>
                            </f:facet>
                            <f:facet name="statusbar">
                              <af:toolbar>
                                <af:outputText value="#{bindings.NTCPublisherTitlesIterator.estimatedRowCount} rows found"/>
                              </af:toolbar>
                            </f:facet>
                            <af:table id="NTCPublisherTitlesTab"
                                      contentDelivery="immediate"
                                      value="#{bindings.NTCPublisherTitlesTable.collectionModel}"
                                      columnStretching="blank"
                                      rowBandingInterval="0"
                                      editingMode="editAll"
                                      columnSelection="multiple"
                                      contextMenuSelect="true"
                                      rowSelection="single"
                                      selectionListener="#{pageFlowScope.NTCPublisherTitlesTable.makeCurrent}"
                                      selectedRowKeys="#{pageFlowScope.NTCPublisherTitlesTable.selectedRow}"
                                      rowDisclosureListener="#{pageFlowScope.NTCPublisherTitlesTable.discloseRow}"
                                      var="row" varStatus="status"
                                      rows="#{bindings.NTCPublisherTitlesTable.rangeSize}"
                                      autoHeightRows="#{bindings.NTCPublisherTitlesTable.estimatedRowCount lt 5 ? 5 : bindings.NTCPublisherTitlesTable.estimatedRowCount lt bindings.NTCPublisherTitlesTable.rangeSize ? bindings.NTCPublisherTitlesTable.estimatedRowCount : 0 }"
                                      fetchSize="#{bindings.NTCPublisherTitlesTable.rangeSize}"
                                      emptyText="#{nls['NO_ROWS_FOUND']}"
                                      queryListener="#{bindings.NTCPublisherTitlesIteratorQuery.processQuery}"
                                      filterModel="#{bindings.NTCPublisherTitlesIteratorQuery.queryDescriptor}"
                                      filterVisible="true"
                                      binding="#{pageFlowScope.NTCPublisherTitlesTable.table}"
                                      partialTriggers=" :::NTCPublisherTitlesPC ::NTCPublisherTitlesFirst ::NTCPublisherTitlesPrevious ::NTCPublisherTitlesNext ::NTCPublisherTitlesLast"
                                      width="892px"
                                      summary="#{nls['TABLE_TITLE_NTCPUBLISHERTITLES']}">
