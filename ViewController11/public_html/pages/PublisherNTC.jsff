                                         noWrap="true" width="100"
                                         sortProperty="TargetTrackingNo"
                                         align="right" rowHeader="false"
                                         filterFeatures="caseInsensitive">
                                <f:facet name="header">
                                  <af:outputLabel value="#{bindings.NTCPublisherTitlesTable.hints.TargetTrackingNo.label}"
                                                  showRequired="#{bindings.NTCPublisherTitlesTable.hints.TargetTrackingNo.mandatory}"
                                                  id="ol0"/>
                                </f:facet>
                                <af:inputListOfValues id="NTCPublisherTitlesTargetTrackingNo"
