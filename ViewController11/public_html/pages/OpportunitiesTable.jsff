                                 rendered="#{!pageFlowScope.createModes.CreateOpportunities and !pageFlowScope.hideSearchArea}"
                                 partialTriggers=" asAreaOppSearchOptions:qsButtonOpp qsAreaOppSearchOptions:asButtonOpp ">
              <f:attribute name="JhsIgnoreChanges" value="true"/>
              <af:query id="asAreaOppSearchOptions"
                        headerText="#{nls['ADVANCED_SEARCH']}" disclosed="true"
                        value="#{bindings.OpportunitiesIteratorPrmOpportunityViewCriteriaAdvQuery.queryDescriptor}"
                        model="#{bindings.OpportunitiesIteratorPrmOpportunityViewCriteriaAdvQuery.queryModel}"
                        queryListener="#{bindings.OpportunitiesIteratorPrmOpportunityViewCriteriaAdvQuery.processQuery}"
                        queryOperationListener="#{bindings.OpportunitiesIteratorPrmOpportunityViewCriteriaAdvQuery.processQueryOperation}"
                        rendered="#{!pageFlowScope.searchOpportunities.quickSearchMode}"
                        rows="1" labelAlignment="top" maxColumns="4">
                <f:facet name="toolbar">
                  <af:commandButton partialSubmit="true" id="qsButtonOpp"
                                    immediate="true"
                                    textAndAccessKey="#{nls['QUICK_SEARCH']}"
                                    action="#{pageFlowScope.searchOpportunities.switchToQuickSearchMode}"/>
                </f:facet>
              </af:query>
              <af:quickQuery id="qsAreaOppSearchOptions"
                             label="#{nls['FILTER_BY']}"
                             value="#{bindings.OpportunitiesIteratorPrmOpportunityViewCriteriaAdvQuery.queryDescriptor}"
                             model="#{bindings.OpportunitiesIteratorPrmOpportunityViewCriteriaAdvQuery.queryModel}"
                             queryListener="#{bindings.OpportunitiesIteratorPrmOpportunityViewCriteriaAdvQuery.processQuery}"
                             rendered="#{pageFlowScope.searchOpportunities.quickSearchMode}">
                <f:facet name="end">
                  <af:commandLink id="asButtonOpp"
                                  textAndAccessKey="#{nls['ADVANCED_SEARCH']}"
                                  partialSubmit="true" immediate="true"
                                  action="#{pageFlowScope.searchOpportunities.switchToAdvancedSearchMode}"/>
                </f:facet>
              </af:quickQuery>
            </af:panelGroupLayout>
          </af:panelHeader>
          <af:panelGroupLayout layout="vertical" id="pgl0">
