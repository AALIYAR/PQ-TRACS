                                    immediate="true" partialSubmit="true">
                      <af:showPopupBehavior popupId="ActDescriptionPopup"/>
                      <af:image source="/jheadstart/images/editor.gif"
                                id="ActDescriptionIcon" clientComponent="true"/>
                    </af:commandLink>
                  </af:panelLabelAndMessage>
                  <af:popup id="ActDescriptionPopup"
