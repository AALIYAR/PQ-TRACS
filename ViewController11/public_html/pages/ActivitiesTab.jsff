                    <af:image source="/afr/shuttleright_dis.png"
                              rendered="#{!(bindings.ActivitiesTabIterator.rangeStart+bindings.ActivitiesTabIterator.currentRowIndexInRange+1 lt bindings.ActivitiesTabIterator.estimatedRowCount)}"
                              id="img2"/>
                    <af:commandToolbarButton actionListener="#{bindings.LastActivitiesTab.execute}"
                                             icon="/afr/shuttlerightall_ena.png"
                                             shortDesc="#{nls['LAST_ROW']}"
