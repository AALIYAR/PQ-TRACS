                           sortProperty="UlrichsSerialId" rowHeader="false"
                           filterFeatures="caseInsensitive">
                  <f:facet name="header">
                    <af:outputLabel value="#{bindings.TitlesTable.hints.UlrichsSerialId.label}"
                                    id="ol8"/>
                  </f:facet>
                  <af:inputText id="cl4"
                                value="#{row.bindings.UlrichsSerialId.inputValue}"
