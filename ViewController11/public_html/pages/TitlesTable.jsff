                    <f:validator binding="#{row.bindings.CatalogNumber.validator}"/>
                  </af:inputText>
                  <af:commandLink id="cl2"
                                  rendered="#{adfFacesContext.outputMode != 'printable'}"
                                  action="TitlesDetails"
                                  text="#{row.CatalogNumber}"></af:commandLink>
                </af:column>
                <af:column id="TtlShortTitleCol" sortable="true"
                           filterable="true" noWrap="true" width="180"
                           sortProperty="ShortTitle" rowHeader="false"
                           filterFeatures="caseInsensitive">
                  <f:facet name="header">
                    <af:outputLabel value="#{bindings.TitlesTable.hints.ShortTitle.label}"
                                    id="ol3"/>
                  </f:facet>
                  <af:inputText id="TtlShortTitle"
                                value="#{row.bindings.ShortTitle.inputValue}"
                                label="#{bindings.TitlesTable.hints.ShortTitle.label}"
                                required="false"
                                columns="#{bindings.TitlesTable.hints.ShortTitle.displayWidth}"
                                maximumLength="30" readOnly="true">
                    <f:validator binding="#{row.bindings.ShortTitle.validator}"/>
                  </af:inputText>
                </af:column>
                <af:column id="TtlContentOwnerLinkCol" sortable="true"
                           noWrap="true" width="180" sortProperty="ContentOwner"
                           rowHeader="false" filterFeatures="caseInsensitive">
                  <f:facet name="header">
                    <af:outputLabel value="#{bindings.TitlesTable.hints.ContentOwner.label}"
                                    id="ol4"/>
                  </f:facet>
                  <af:inputText id="cl3"
                                value="#{row.bindings.ContentOwner.inputValue}"
                                label="#{bindings.TitlesTable.hints.ContentOwner.label}"
                                required="true"
                                columns="#{bindings.TitlesTable.hints.ContentOwner.displayWidth}"
                                maximumLength="400" readOnly="true"
                                rendered="#{adfFacesContext.outputMode eq 'printable'}">
                    <f:validator binding="#{row.bindings.ContentOwner.validator}"/>
                  </af:inputText>
                  <af:commandLink id="TtlContentOwnerLink"
                                  text="#{row.bindings.ContentOwner.attributeValue}"
                                  action="LaunchTab"
                                  rendered="#{adfFacesContext.outputMode != 'printable'}">
                    <af:setActionListener from="ContentOwner"
                                          to="#{jhsDynTabLauncher.taskFlowName}"/>
                    <af:setActionListener from="#{bindings.TtlContentOwnerId.inputValue}"
                                          to="#{jhsDynTabLauncher.tabUniqueIdentifier}"/>
                    <af:setActionListener from="CO - #{bindings.TtlContentOwnerLink.inputValue}"
                                          to="#{jhsDynTabLauncher.label}"/>
                    <af:setActionListener from="#{bindings.TtlContentOwnerId.inputValue}"
                                          to="#{jhsDynTabLauncher.parameters.rowKeyValueContentOwner}"/>
                    <af:setActionListener from="#{jhsDynTabLauncher}"
                                          to="#{pageFlowScope.parentContext.jhsDynTabLauncher}"/>
                  </af:commandLink>
                </af:column>
                <af:column id="TtlSiteCol" sortable="true" filterable="true"
                           noWrap="true" width="180" sortProperty="Site"
                           rowHeader="false" filterFeatures="caseInsensitive">
                  <f:facet name="header">
                    <af:outputLabel value="#{bindings.TitlesTable.hints.Site.label}"
                                    id="ol5"/>
                  </f:facet>
                  <af:inputText id="TtlSite"
                                value="#{row.bindings.Site.inputValue}"
                                label="#{bindings.TitlesTable.hints.Site.label}"
                                required="false"
                                columns="#{bindings.TitlesTable.hints.Site.displayWidth}"
                                readOnly="true">
                    <f:validator binding="#{row.bindings.Site.validator}"/>
                  </af:inputText>
                </af:column>
                <af:column id="TtlContentOwnerCountCol" sortable="true"
                           filterable="true" noWrap="true" width="100"
                           sortProperty="ContentOwnerCount" rowHeader="false"
                           filterFeatures="caseInsensitive">
                  <f:facet name="header">
                    <af:outputLabel value="#{bindings.TitlesTable.hints.ContentOwnerCount.label}"
                                    id="ol6"/>
                  </f:facet>
                  <af:inputText id="TtlContentOwnerCount"
                                value="#{row.bindings.ContentOwnerCount.inputValue}"
