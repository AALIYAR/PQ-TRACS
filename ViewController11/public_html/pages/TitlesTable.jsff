                                columns="#{bindings.TitlesTable.hints.ContentOwnerCount.displayWidth}"
                                maximumLength="4" readOnly="true">
                    <f:validator binding="#{row.bindings.ContentOwnerCount.validator}"/>
                    <af:convertNumber groupingUsed="false"
                                      pattern="#{bindings.TitlesTable.hints.ContentOwnerCount.format}"/>
