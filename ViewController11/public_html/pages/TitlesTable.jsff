<?xml version='1.0' encoding="UTF-8"?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1"
          xmlns:h="http://java.sun.com/jsf/html"
          xmlns:f="http://java.sun.com/jsf/core"
          xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
          xmlns:trh="http://myfaces.apache.org/trinidad/html"
          xmlns:tr="http://myfaces.apache.org/trinidad"
          xmlns:dvt="http://xmlns.oracle.com/dss/adf/faces">
  <jsp:directive.page contentType="text/html;charset=UTF-8"/>
  <af:pageTemplate id="pt"
                   viewId="/common/pageTemplates/JhsRegionTemplateNoStretch.jspx">
    <f:facet name="pageContent">
      <!-- Pushed "disabled" on stretchContext, now: Stretching disabled on group level. -->
      <af:panelGroupLayout id="pcpg" layout="scroll">
        <af:panelGroupLayout layout="vertical" id="TtlTopLc"
                             partialTriggers=" TtlCancelButton0 asAreaTtlSearchOptions qsAreaTtlSearchOptions"
                             rendered="#{jhsUserRoles['AM, PM, AST, GCA, RO, RM, ADMIN, USER, ReadOnly, ActOnly, AllButTitl, AllButAdmn, OVR, RORoyalty']}">
          <f:facet name="separator">
            <af:spacer width="10" height="10" id="sp0"/>
          </f:facet>
          <!-- Pushed "disabled" on stretchContext, now: Stretching disabled on group level. -->
          <af:panelHeader id="ph0" text="#{nls['TABLE_TITLE_TITLES']}">
            <f:facet name="toolbar">
              <af:toolbox id="tbox0">
                <af:toolbar id="tbar0">
                  <af:group id="gr0">
                    <af:commandButton actionListener="#{CreateTitles.execute}"
                                      action="CreateTtl"
                                      textAndAccessKey="#{nls['NEW_BUTTON_LABEL_TITLES']}"
                                      rendered="#{(true) and (jhsUserRoles['RM, ADMIN, AllButAdmn'])}"
                                      id="TtlNewButton"></af:commandButton>
                  </af:group>
                  <af:group id="gr1"></af:group>
                  <af:group id="gr2">
                    <af:commandButton textAndAccessKey="#{nls['CANCEL']}"
                                      immediate="true" partialSubmit="true"
                                      rendered="#{!pageFlowScope.hideCancelButton}"
                                      actionListener="#{jhsRollback.execute}"
                                      action="Cancel" id="TtlCancelButton0"></af:commandButton>
                  </af:group>
                </af:toolbar>
              </af:toolbox>
            </f:facet>
            <f:facet name="context"></f:facet>
            <af:panelGroupLayout id="modelSearchAreaTtl"
                                 rendered="#{!pageFlowScope.createModes.CreateTitles and !pageFlowScope.hideSearchArea}"
                                 partialTriggers=" asAreaTtlSearchOptions:qsButtonTtl qsAreaTtlSearchOptions:asButtonTtl ">
              <f:attribute name="JhsIgnoreChanges" value="true"/>
              <af:query id="asAreaTtlSearchOptions"
                        headerText="#{nls['ADVANCED_SEARCH']}" disclosed="true"
                        value="#{bindings.TitlesIteratorTitleViewCriteriaAdvQuery.queryDescriptor}"
                        model="#{bindings.TitlesIteratorTitleViewCriteriaAdvQuery.queryModel}"
                        queryListener="#{bindings.TitlesIteratorTitleViewCriteriaAdvQuery.processQuery}"
                        queryOperationListener="#{bindings.TitlesIteratorTitleViewCriteriaAdvQuery.processQueryOperation}"
                        rendered="#{!pageFlowScope.searchTitles.quickSearchMode}"
                        rows="1" labelAlignment="top" maxColumns="4">
                <f:facet name="toolbar">
                  <af:commandButton partialSubmit="true" id="qsButtonTtl"
                                    immediate="true"
                                    textAndAccessKey="#{nls['QUICK_SEARCH']}"
                                    action="#{pageFlowScope.searchTitles.switchToQuickSearchMode}"/>
                </f:facet>
              </af:query>
              <af:quickQuery id="qsAreaTtlSearchOptions"
                             label="#{nls['FILTER_BY']}"
                             value="#{bindings.TitlesIteratorTitleViewCriteriaAdvQuery.queryDescriptor}"
                             model="#{bindings.TitlesIteratorTitleViewCriteriaAdvQuery.queryModel}"
                             queryListener="#{bindings.TitlesIteratorTitleViewCriteriaAdvQuery.processQuery}"
                             rendered="#{pageFlowScope.searchTitles.quickSearchMode}">
                <f:facet name="end">
                  <af:commandLink id="asButtonTtl"
                                  textAndAccessKey="#{nls['ADVANCED_SEARCH']}"
                                  partialSubmit="true" immediate="true"
                                  action="#{pageFlowScope.searchTitles.switchToAdvancedSearchMode}"/>
                </f:facet>
              </af:quickQuery>
            </af:panelGroupLayout>
          </af:panelHeader>
          <af:panelGroupLayout layout="vertical" id="pgl0">
            <af:panelCollection id="TtlPC" featuresOff=" "
                                styleClass="AFStretchWidth">
              <f:facet name="menus">
                <af:menu textAndAccessKey="#{nls['ACTION_MENU_LABEL']}"
                         id="menu0">
                  <af:menu textAndAccessKey="#{nls['GO_TO_MENU_LABEL']}"
                           id="menu1">
                    <af:commandMenuItem id="mi0"
                                        textAndAccessKey="#{nls['DETAIL_VIEW_BUTTON_LABEL']}"
                                        action="TitlesDetails"></af:commandMenuItem>
                    <af:group id="Ttltbb"
                              rendered="#{ (bindings.TitlesIterator.viewObject!=null and bindings.TitlesIterator.viewObject.currentRow!=null)}">
                      <af:commandMenuItem actionListener="#{bindings.FirstTitles.execute}"
                                          id="TtlFirst" partialSubmit="true"
                                          icon="/afr/shuttleleftall_ena.png"
                                          textAndAccessKey="#{nls['FIRST_ROW']}"></af:commandMenuItem>
                      <af:commandMenuItem actionListener="#{bindings.PreviousTitles.execute}"
                                          icon="/afr/shuttleleft_ena.png"
                                          textAndAccessKey="#{nls['PREVIOUS_ROW']}"
                                          id="TtlPrevious" partialSubmit="true"></af:commandMenuItem>
                      <af:commandMenuItem actionListener="#{bindings.NextTitles.execute}"
                                          icon="/afr/shuttleright_ena.png"
                                          textAndAccessKey="#{nls['NEXT_ROW']}"
                                          id="TtlNext" partialSubmit="true"></af:commandMenuItem>
                      <af:commandMenuItem actionListener="#{bindings.LastTitles.execute}"
                                          icon="/afr/shuttlerightall_ena.png"
                                          textAndAccessKey="#{nls['LAST_ROW']}"
                                          id="TtlLast" partialSubmit="true"></af:commandMenuItem>
                    </af:group>
                  </af:menu>
                  <af:commandMenuItem partialSubmit="true"
                                      textAndAccessKey="#{nls['EXPORT']}"
                                      icon="/jheadstart/images/excel.png"
                                      immediate="true" id="cmi0">
                    <af:exportCollectionActionListener exportedId="TtlTab"
                                                       type="excelHTML"
                                                       filename="Titles.xls"/>
                  </af:commandMenuItem>
                </af:menu>
              </f:facet>
              <f:facet name="toolbar">
                <af:toolbar id="tbar1">
                  <af:commandImageLink immediate="true" partialSubmit="true"
                                       id="cl0" shortDesc="#{nls['EXPORT']}"
                                       icon="/jheadstart/images/excel.png">
                    <af:exportCollectionActionListener exportedId="TtlTab"
                                                       type="excelHTML"
                                                       filename="Titles.xls"/>
                  </af:commandImageLink>
                  <af:commandToolbarButton text="Print" id="TtlPrint"
                                           shortDesc="Opens a new tab with printer friendly format.">
                    <af:showPrintablePageBehavior/>
                  </af:commandToolbarButton>
                  <af:popup id="noSelectionPopup"
                            binding="#{pageFlowScope.TitlesTable.noSelectionPopup}">
                    <af:dialog modal="true" type="cancel"
                               title="Select Titles First">
                      <af:outputText value="You must select at least one row."/>
                    </af:dialog>
                  </af:popup>
                  <af:popup id="addNotePopup"
                            binding="#{pageFlowScope.TitlesTable.addNotePopup}">
                    <af:dialog modal="true" type="okCancel" title="Enter Note"
                               dialogListener="#{pageFlowScope.TitlesTable.handleAddNoteOK}">
                      <af:selectOneChoice value="#{pageFlowScope.TitlesTable.newNoteType}"
                                          label="Title Note" required="false"
                                          shortDesc="#{bindings.TitleNoteTypeLookup.hints.tooltip}"
                                          id="noteTypeSelect">
                        <f:selectItems value="#{bindings.TitleNoteTypeLookup.items}"
                                       id="noteTypeItems"/>
                      </af:selectOneChoice>
                      <af:inputText id="noteToAdd" label="Note"
                                    converter="oracle.genericDomain" rows="10"
                                    columns="80"
                                    value="#{pageFlowScope.TitlesTable.newNote}"/>
                      <af:outputText value="This note will be applied to the following catalog number(s): #{pageFlowScope.TitlesTable.catalogList}"/>
                    </af:dialog>
                  </af:popup>
                  <af:commandButton id="TtlAddNotes" textAndAccessKey="Add Note"
                                    actionListener="#{pageFlowScope.TitlesTable.addNotes}"></af:commandButton>
                </af:toolbar>
              </f:facet>
              <f:facet name="statusbar">
                <af:toolbar>
                  <af:outputText value="#{bindings.TitlesIterator.estimatedRowCount} rows found"/>
                </af:toolbar>
              </f:facet>
              <af:table id="TtlTab" contentDelivery="immediate"
                        value="#{bindings.TitlesTable.collectionModel}"
                        columnStretching="blank" rowBandingInterval="0"
                        editingMode="editAll" columnSelection="multiple"
                        contextMenuSelect="true"
                        selectionListener="#{bindings.TitlesTable.collectionModel.makeCurrent}"
                        rowSelection="multiple"
                        rowDisclosureListener="#{pageFlowScope.TitlesTable.discloseRow}"
                        var="row" varStatus="status"
                        rows="#{bindings.TitlesTable.rangeSize}"
                        autoHeightRows="#{bindings.TitlesTable.estimatedRowCount}"
                        fetchSize="#{bindings.TitlesTable.rangeSize}"
                        emptyText="#{nls['NO_ROWS_FOUND']}"
                        queryListener="#{bindings.TitlesIteratorTitleViewCriteriaAdvQuery.processQuery}"
                        filterModel="#{bindings.TitlesIteratorTitleViewCriteriaAdvQuery.queryDescriptor}"
                        filterVisible="true"
                        binding="#{pageFlowScope.TitlesTable.table}"
                        partialTriggers=":::qsAreaTtlSearchOptions :::asAreaTtlSearchOptions :::TtlPC ::TtlFirst ::TtlPrevious ::TtlNext ::TtlLast"
                        styleClass="AFStretchWidth"
                        summary="#{nls['TABLE_TITLE_TITLES']}">
                <f:facet name="contextMenu">
                  <af:popup id="TtlTablePopup" childCreation="deferred">
                    <af:menu id="menu2">
                      <af:menu text="#{nls['GO_TO_MENU_LABEL']}" id="menu3">
                        <af:commandMenuItem id="mi1"
                                            textAndAccessKey="#{nls['DETAIL_VIEW_BUTTON_LABEL']}"
                                            action="TitlesDetails"></af:commandMenuItem>
                      </af:menu>
                    </af:menu>
                  </af:popup>
                </f:facet>
                <af:column id="TtlTargetTrackingNoCol" sortable="true"
                           filterable="true" noWrap="true" width="100"
                           sortProperty="TargetTrackingNo" align="right"
                           rowHeader="false" filterFeatures="caseInsensitive">
                  <f:facet name="header">
                    <af:outputLabel value="#{bindings.TitlesTable.hints.TargetTrackingNo.label}"
                                    id="ol0"/>
                  </f:facet>
                  <af:inputText id="TtlTargetTrackingNo"
                                value="#{row.bindings.TargetTrackingNo.inputValue}"
                                label="#{bindings.TitlesTable.hints.TargetTrackingNo.label}"
                                required="#{bindings.TitlesTable.hints.TargetTrackingNo.mandatory}"
                                columns="#{bindings.TitlesTable.hints.TargetTrackingNo.displayWidth}"
                                maximumLength="38" readOnly="true"
                                shortDesc="#{bindings.TitlesTable.hints.TargetTrackingNo.tooltip}">
                    <f:validator binding="#{row.bindings.TargetTrackingNo.validator}"/>
                    <af:convertNumber groupingUsed="false"
                                      pattern="#{bindings.TitlesTable.hints.TargetTrackingNo.format}"/>
                  </af:inputText>
                </af:column>
                <af:column id="TtlCopyOfLongTitleCol" sortable="true"
