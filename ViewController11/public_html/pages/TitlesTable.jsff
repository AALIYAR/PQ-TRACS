                                required="false"
                                columns="#{bindings.TitlesTable.hints.CatalogRoot.displayWidth}"
                                maximumLength="20" readOnly="true">
                    <f:validator binding="#{row.bindings.CatalogRoot.validator}"/>
                  </af:inputText>
                </af:column>
