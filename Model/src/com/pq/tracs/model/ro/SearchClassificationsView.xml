   <SQLQuery>
      <![CDATA[SELECT Classification.ID, 
       Classification.TITLE_ID, 
       NVL(Classification.TITLE_ID, title1.id) AS TITLE_ID2,
       Classification.CLASSIFICATION_TYPE, 
       Classification.CLASSIFICATION_NUMBER,
       Classification.PRIMARY_FLAG, 
       nvl(title.LONG_TITLE,title1.long_title) as LONG_TITLE,
       ovr.CONTENT_SOURCE,
       ovr.PMID,
       ovr.PM_TITLE,       
       Classification.CREATED_BY, 
       Classification.CREATED_DATE, 
       Classification.LAST_UPDATED_BY, 
       Classification.LAST_UPDATED_DATE,
       Classification.TITLE_RECORD_ID,
       ovr.OVR_ID,
       nvl(title.CATALOG_ROOT, title1.CATALOG_ROOT) AS CATALOG_ROOT,
       NVL(title.CATALOG_NUMBER, TITLE1.CATALOG_NUMBER) AS CATALOG_NUMBER,
       1 link_value
FROM TRAX_CLASSIFICATION Classification 
LEFT OUTER JOIN trax_title_record ovr
                on Classification.title_record_id = ovr.id
LEFT OUTER JOIN trax_title title
                on Classification.title_id = title.id
                and Title.ACTIVE_FLAG ='Y'
LEFT OUTER JOIN TRAX_TITLE title1
on ovr.title_id = title1.id
WHERE  title.LONG_TITLE IS NOT NULL 
OR ovr.PM_TITLE IS NOT NULL]]>
   </SQLQuery>
