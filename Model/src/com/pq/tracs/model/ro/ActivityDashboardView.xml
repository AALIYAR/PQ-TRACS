  <SQLQuery>
    <![CDATA[select act.ID ACTIVITY_ID,
       act.TYPE, 
       act.SUB_TYPE,  
       act.DESCRIPTION,
       act.DUE_DATE, 
       ju.ID USER_ID, 
       aus.USER_TYPE, 
       act.STATUS,
       TraxTitle.CATALOG_NUMBER,
       TraxTitle.LONG_TITLE,
       aco.content_owner_id, 
       tco.content_owner_no, 
       tco.name, 
       tco.site,
       act.created_by, 
       act.created_date,
       TraxTitle.ID AS TTL_ID,
       TraxTitle.TARGET_TRACKING_NO,
       TraxAddress.COUNTRY
from trax_activity act
    inner join trax_activity_user aus
        inner join jhs_users ju
            on aus.JHS_USERS_ID = ju.ID
        on act.ID = aus.ACTIVITY_ID
    left outer join trax_activity_content_owner aco
        inner join trax_content_owner tco
          LEFT OUTER JOIN TRAX_ADDRESS TraxAddress
            ON tco.ID = TraxAddress.CONTENT_OWNER_ID
            AND TraxAddress.PRIMARY_FLAG = 'Y'
            on aco.content_owner_id = tco.id
        on act.id = aco.activity_id
        and aco.primary_flag = 'Y'
    left outer join TRAX_ACTIVITY_TITLE TraxActivityTitle
    INNER JOIN TRAX_TITLE TraxTitle        
        ON TraxActivityTitle.TITLE_ID = TraxTitle.ID
        on act.ID = TraxActivityTitle.ACTIVITY_ID
        and TraxActivityTitle.PRIMARY_FLAG='Y'
where act.TYPE <> 'OppReminder'
and act.STATUS in ('Open', 'In Progress', 'New')]]>
  </SQLQuery>
