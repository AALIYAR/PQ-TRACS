  <SQLQuery>
    <![CDATA[SELECT DISTINCT PrmTitleProduct.PRD_ID,
       PrmTitleProduct.TITLE_ID,
       PrmTargetedProduct.ID, 
       PrmTargetedProduct.NAME, 
       PrmTargetedProduct.PRODUCT_FAMILY, 
       JhsUsers.DISPLAY_NAME AS PRODUCT_MANAGER,
       PrmTargetedProduct.CREATED_BY, 
       PrmTargetedProduct.CREATED_DATE, 
       PrmTargetedProduct.LAST_UPDATED_BY, 
       PrmTargetedProduct.LAST_UPDATED_DATE
FROM PRM_TARGETED_PRODUCT PrmTargetedProduct
    INNER JOIN PRM_TITLE_PRODUCT PrmTitleProduct
        ON PrmTargetedProduct.PRD_ID = PrmTitleProduct.PRD_ID
    LEFT OUTER JOIN PRM_TARGETED_PRODUCT_MANAGER PrmTargetedProductManager
        INNER JOIN PRM_ALLIANCE_MANAGER PrmAllianceManager
            INNER JOIN JHS_USERS JhsUsers
                ON PrmAllianceManager.JHS_USERS_ID = JhsUsers.ID
            ON PrmTargetedProductManager.PRM_USER_ID = PrmAllianceManager.ID
        ON PrmTargetedProduct.ID = PrmTargetedProductManager.PRM_TARGETED_PRODUCT_ID
        AND PrmTargetedProductManager.REMOVED_DATE IS NULL]]>
  </SQLQuery>
