  <SQLQuery>
    <![CDATA[SELECT PrmTargetedProductTitle.ID, 
       PrmTargetedProductTitle.PRM_TARGETED_PRODUCT_ID, 
       PrmTargetedProductTitle.PRM_TARGET_GROUP_ID, 
       PrmTargetedProductTitle.TITLE_ID, 
       PrmTargetedProductTitle.ADDED_DATE, 
       PrmTargetedProductTitle.REMOVED_DATE, 
       PrmTargetedProductTitle.CREATED_BY, 
       PrmTargetedProductTitle.CREATED_DATE, 
       PrmTargetedProductTitle.LAST_UPDATED_BY, 
       PrmTargetedProductTitle.LAST_UPDATED_DATE, 
       TraxTitle.ID AS ID1, 
       TraxTitle.CONTENT_OWNER_ID, 
       TraxTitle.TARGET_TRACKING_NO,
       TraxTitle.CATALOG_NUMBER, 
       TraxTitle.LONG_TITLE, 
       TraxTitle.AUTHOR,
       TraxContentOwner.ID AS ID2,
       TraxContentOwner.NAME,
       TraxContentOwner.CONTENT_OWNER_NO,
       PrmTargetedProduct.PRD_ID,
       PrmTargetedProduct.NAME Product_Name,
       PrmTargetedProduct.PRODUCT_FAMILY,
       PrmTargetGroup.GROUP_NAME TARGET_GROUP,
       CurrentCatalogTitle.ID AS CURRENT_CATALOG_ID,
       CurrentCatalogTitle.CATALOG_NUMBER AS CURRENT_CATALOG_NUMBER
FROM PRM_TARGETED_PRODUCT_TITLE PrmTargetedProductTitle
    INNER JOIN TRAX_TITLE TraxTitle
        LEFT OUTER JOIN TRAX_CONTENT_OWNER TraxContentOwner
            ON TraxTitle.CONTENT_OWNER_ID = TraxContentOwner.ID
        LEFT OUTER JOIN TRAX_TITLE CurrentCatalogTitle
            ON TraxTitle.CURRENT_CATALOG_ID = CurrentCatalogTitle.ID
        ON PrmTargetedProductTitle.TITLE_ID = TraxTitle.ID
    LEFT OUTER JOIN PRM_TARGETED_PRODUCT PrmTargetedProduct
        ON PrmTargetedProductTitle.PRM_TARGETED_PRODUCT_ID = PrmTargetedProduct.ID
    LEFT OUTER JOIN PRM_OPPORTUNITY_TARGET PrmOpportunityTarget
        ON PrmTargetedProductTitle.ID = PrmOpportunityTarget.PRM_TARGETED_PRODUCT_TITLE_ID
    LEFT OUTER JOIN PRM_TARGET_GROUP PrmTargetGroup
        ON PrmTargetedProductTitle.PRM_TARGET_GROUP_ID = PrmTargetGroup.ID
WHERE PrmOpportunityTarget.ID IS NULL
OR PrmOpportunityTarget.REMOVED_DATE < SYSDATE]]>
  </SQLQuery>
