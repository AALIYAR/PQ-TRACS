   <SQLQuery><![CDATA[
SELECT Contract.ID, 
       Contract.CONTENT_OWNER_ID, 
       Contract.CONTRACT_NUMBER, 
       Contract.FORM, 
       Contract.STATUS, 
       Contract.TYPE, 
       Contract.DIVISION, 
       Contract.EFFECTIVE_DATE, 
       Contract.EXPIRATION_DATE, 
       Contract.TERM_LENGTH, 
       Contract.CANCELLATION_NOTICE_TERM, 
       Contract.ROLLOVER_DATE, 
       Contract.CANCELLATION_DATE, 
       Contract.CANCELLATION_EFFECTIVE_DATE, 
       Contract.AUTO_RENEWS_FLAG, 
       Contract.CONTRACT_SIGNED_DATE, 
       Contract.AUTO_RENEW_TERM_MONTHS, 
       Contract.RELATIONSHIP_OWNER, 
       Contract.ACTIVE_FLAG,
       ContentOwner.NAME Content_owner,
(select concat(first_name,concat(' ',Last_name)) from TRAX_CONTACT a where a.id=Contract.AGREEMENT_CONTACT_ID) AGREEMENT_Name,
(select concat(first_name,concat(' ',Last_name)) from TRAX_CONTACT a where a.id=Contract.QA_CONTACT_ID) QA_Name,
(select concat(first_name,concat(' ',Last_name)) from TRAX_CONTACT a where a.id=Contract.TECHNICAL_CONTACT_ID) TECHNICAL_Name,
       ContentOwner.SITE
FROM TRAX_CONTRACT Contract, TRAX_CONTENT_OWNER ContentOwner
WHERE Contract.CONTENT_OWNER_ID = ContentOwner.ID
AND (NVL(Contract.ACTIVE_FLAG,'Y')='Y' OR :SHOW_ALL='ALL')
AND Contract.IS_TEMPLATE_FLAG = 'N'
   ]]></SQLQuery>
