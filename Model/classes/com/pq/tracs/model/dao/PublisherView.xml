   <SQLQuery>
      <![CDATA[SELECT Publisher.ID, 
       Publisher.NAME, 
       Publisher.PUB_ID, 
       Publisher.SORT_NAME, 
       Publisher.URL, 
       Publisher.URL AS URL_2,
       Publisher.NOTES, 
       Publisher.ORG_ID,
       Publisher.MS_ID,  
       Publisher.CONTENT_OWNER_ID,
       ContentOwner.NAME CONTENT_OWNER_NAME,
       ContentOwner.NAME CONTENT_OWNER_NAME_2,
       ContentOwner.SITE,
       ContentOwner.CONTENT_OWNER_NO,
       Publisher.CREATED_BY, 
       Publisher.CREATED_DATE, 
trunc(Publisher.CREATED_DATE) as created_date_search,
       Publisher.LAST_UPDATED_BY, 
       Publisher.LAST_UPDATED_DATE,
trunc(Publisher.LAST_UPDATED_DATE) as last_updated_date_search
FROM TRAX_PUBLISHER Publisher
    LEFT OUTER JOIN TRAX_CONTENT_OWNER ContentOwner
        ON Publisher.CONTENT_OWNER_ID = ContentOwner.ID]]>
   </SQLQuery>
