   <SQLQuery><![CDATA[SELECT Activity.ID, 
       ContentOwner.NAME ACCOUNT_NAME,
       ContentOwner.SITE ACCOUNT_SITE, 
       Contract.CONTRACT_NUMBER,
       Title.CATALOG_NUMBER,  
       Title.SHORT_TITLE, 
       Activity.TYPE, 
       Activity.SUB_TYPE, 
       ExUsers.DISPLAY_NAME,
       ExUsers.USERNAME AS OWNER, 
       Activity.STATUS,      
       Activity.PRIORITY,  
       Activity.EXPECTED_START_DATE, 
       Activity.ACTUAL_START_DATE,
       Activity.DUE_DATE, 
       Activity.COMPLETION_DATE, 
       Activity.HOURS_SPENT, 
       Activity.DESCRIPTION, 
       Activity.NOTES,
       Activity.CREATED_BY, 
       Activity.CREATED_DATE, 
       Activity.LAST_UPDATED_BY, 
       Activity.LAST_UPDATED_DATE, 
       Activity.EMAIL_TEXT,
       ContentOwner.ID AS ACCOUNT_ID,  
       Contract.ID AS CONTRACT_ID, 
       Title.ID AS TITLE_ID,
       ExUsers.ID AS USER_ID
FROM TRAX_ACTIVITY Activity
    LEFT OUTER JOIN TRAX_ACTIVITY_CONTENT_OWNER ActivityContentOwner
        INNER JOIN TRAX_CONTENT_OWNER ContentOwner 
            ON ActivityContentOwner.CONTENT_OWNER_ID = ContentOwner.ID
        ON Activity.ID = ActivityContentOwner.ACTIVITY_ID
        AND ActivityContentOwner.PRIMARY_FLAG = 'Y'
    LEFT OUTER JOIN TRAX_ACTIVITY_CONTRACT ActivityContract
        INNER JOIN TRAX_CONTRACT Contract 
            ON ActivityContract.CONTRACT_ID = Contract.ID
        ON Activity.ID = ActivityContract.ACTIVITY_ID
        AND ActivityContract.PRIMARY_FLAG = 'Y'
    LEFT OUTER JOIN TRAX_ACTIVITY_TITLE ActivityTitle
        INNER JOIN TRAX_TITLE Title
            ON ActivityTitle.TITLE_ID = Title.ID
        ON Activity.ID = ActivityTitle.ACTIVITY_ID
        AND ActivityTitle.PRIMARY_FLAG = 'Y'
    LEFT OUTER JOIN TRAX_ACTIVITY_USER ActivityUser
        INNER JOIN JHS_USERS ExUsers
            ON ActivityUser.JHS_USERS_ID = ExUsers.ID
        ON Activity.ID = ActivityUser.ACTIVITY_ID
        AND ActivityUser.USER_TYPE = 'Owner'
WHERE Activity.TYPE <> 'OppReminder']]></SQLQuery>
