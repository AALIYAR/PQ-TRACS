  <SQLQuery>
    <![CDATA[SELECT TraxPreResult.ID, 
       TraxPreResult.PRM_OPPORTUNITY_ID, 
       TraxPreResult.OUTCOME_NO, 
       TraxPreResult.PRE_BATCH_ID, 
       TraxPreResult.CHILD_ID, 
       TraxPreResult.LICENSOR, 
       TraxPreResult.PQ_PLATFORM, 
       TraxPreResult.PQ_MFG_FLOW, 
       TraxPreResult.PQ_RECEIPT, 
       TraxPreResult.ILLUMINA_PLATFORM, 
       TraxPreResult.ILLUMINA_MFG_FLOW, 
       TraxPreResult.ILLUMINA_RECEIPT, 
       TraxPreResult.PREF_IMAGE_STORAGE, 
       TraxPreResult.MFG_PROCESS, 
       TraxPreResult.RAMP_TYPE, 
       TraxPreResult.OUTCOME_NOTES, 
       TraxPreResult.DATE_PRE_COMPLETED, 
       TraxPreResult.PRE_PERFORMED_BY, 
       TraxPreResult.PRE_TIMING, 
       TraxPreResult.PQ_TARGETED, 
       TraxPreResult.ILLUMINA_TARGETED, 
       TraxPreResult.ABSTRACT AS ABSTRACT11, 
       TraxPreResult.INDEXING, 
       TraxPreResult.UNSTRUCTURED_TEXT, 
       TraxPreResult.FULLTEXT, 
       TraxPreResult.TEXT_AND_GRAPHICS, 
       TraxPreResult.ARTICLE_IMAGE, 
       TraxPreResult.FULL_PAGE_IMAGE, 
       TraxPreResult.CITATION_REFERENCE, 
       TraxPreResult.MULTIMEDIA_VIDEO, 
       TraxPreResult.MULTIMEDIA_TEXT_BASED, 
       TraxPreResult.ILLUSTRATA_OBJECTS, 
       TraxPreResult.CREATED_BY, 
       TraxPreResult.CREATED_DATE, 
       TraxPreResult.LAST_UPDATED_BY, 
       TraxPreResult.LAST_UPDATED_DATE,
  OPS.STATUS_DATE PRE_REQUEST_DATE,
       OPS.CREATED_BY PRE_REQUESTED_BY
FROM TRAX_PRE_RESULT TraxPreResult
  INNER JOIN PRM_OPPORTUNITY OPP
    INNER JOIN PRM_OPPORTUNITY_PROCESS_STATUS OPS
      ON OPP.ID = OPS.PRM_OPPORTUNITY_ID
      AND OPS.STATUS_TYPE = 'PRE Status'
      AND OPS.STATUS_CODE = 'Submitted'  AND OPS.STATUS_DATE in 
      ( select ( max(OPS.STATUS_DATE)) from PRM_OPPORTUNITY_PROCESS_STATUS OPS
          inner join TRAX_PRE_RESULT TraxPreResult ON
TraxPreResult.PRM_OPPORTUNITY_ID = OPS.PRM_OPPORTUNITY_ID
     where      OPS.STATUS_TYPE = 'PRE Status'
     AND OPS.STATUS_CODE = 'Submitted'
     group by ops.prm_opportunity_id ) 
    ON TraxPreResult.PRM_OPPORTUNITY_ID = OPP.ID]]>
  </SQLQuery>
