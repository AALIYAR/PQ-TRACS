  <SQLQuery>
    <![CDATA[SELECT TraxActivityOpportunity.ID, 
       TraxActivityOpportunity.ACTIVITY_ID, 
       TraxActivityOpportunity.PRM_OPPORTUNITY_ID, 
       TraxActivityOpportunity.PRIMARY_FLAG, 
       TraxActivityOpportunity.ADDED_DATE, 
       TraxActivityOpportunity.REMOVED_DATE, 
       TraxActivityOpportunity.CREATED_BY, 
       TraxActivityOpportunity.CREATED_DATE, 
       TraxActivityOpportunity.LAST_UPDATED_BY, 
       TraxActivityOpportunity.LAST_UPDATED_DATE,
       PrmOpportunity.ID OPP_ID,
       PrmOpportunity.OPPORTUNITY_NO,
       PrmOpportunity.NAME OPPORTUNITY_NAME,
       JhsUsers.DISPLAY_NAME Alliance_Manager
FROM TRAX_ACTIVITY_OPPORTUNITY TraxActivityOpportunity
    INNER JOIN PRM_OPPORTUNITY PrmOpportunity
        ON TraxActivityOpportunity.PRM_OPPORTUNITY_ID = PrmOpportunity.ID
    LEFT OUTER JOIN PRM_OPPORTUNITY_ALLIANCE_MGR PrmOpportunityAllianceManager
        INNER JOIN PRM_ALLIANCE_MANAGER PrmAllianceManager
            INNER JOIN JHS_USERS JhsUsers
                ON PrmAllianceManager.JHS_USERS_ID = JhsUsers.ID
            ON PrmOpportunityAllianceManager.PRM_ALLIANCE_MANAGER_ID = PrmAllianceManager.ID
        ON PrmOpportunity.ID = PrmOpportunityAllianceManager.PRM_OPPORTUNITY_ID
        AND PrmOpportunityAllianceManager.END_DATE IS NULL]]>
  </SQLQuery>
