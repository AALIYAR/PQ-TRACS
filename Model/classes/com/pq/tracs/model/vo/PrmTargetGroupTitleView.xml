  <SQLQuery>
    <![CDATA[SELECT PrmTargetGroupTitle.ID, 
       PrmTargetGroupTitle.PRM_TARGET_GROUP_ID, 
       PrmTargetGroupTitle.TITLE_ID, 
       PrmTargetGroupTitle.ADDED_DATE, 
       PrmTargetGroupTitle.REMOVED_DATE, 
       PrmTargetGroupTitle.CREATED_BY, 
       PrmTargetGroupTitle.CREATED_DATE, 
       PrmTargetGroupTitle.LAST_UPDATED_BY, 
       PrmTargetGroupTitle.LAST_UPDATED_DATE,
       TraxTitle.ID Id1,
       TraxTitle.LONG_TITLE,
       TraxTitle.CATALOG_NUMBER,
       TraxTitle.OPEN_ACCESS,
       TraxTitle.TARGET_TRACKING_NO,
       TraxTitleSubject.SUBJECT AS SUBJECT_CODE,
       TraxLookupCode.DISPLAY_VALUE AS SUBJECT,
       TraxTitleSubject.SUBJECT_TYPE,
       TraxContentOwner.ID AS ACCOUNT_ID,
       TraxContentOwner.CONTENT_OWNER_NO AS ACCOUNT_NO,
       TraxContentOwner.NAME AS ACCOUNT_NAME,
       TraxAddress.COUNTRY AS ACCOUNT_COUNTRY,
       listagg(TraxTitleIndexInclusion.INDEX_NAME, ', ') within group (order by index_name) AS EXTERNAL_INDEXES
FROM PRM_TARGET_GROUP_TITLE PrmTargetGroupTitle
    INNER JOIN TRAX_TITLE TraxTitle
        LEFT OUTER JOIN TRAX_TITLE_INDEX_INCLUSION TraxTitleIndexInclusion
            ON TraxTitle.ID = TraxTitleIndexInclusion.TITLE_ID
        LEFT OUTER JOIN TRAX_TITLE_SUBJECT TraxTitleSubject
            INNER JOIN TRAX_LOOKUP_CODE TraxLookupCode
                INNER JOIN TRAX_LOOKUP_CODE_SET TraxLookupCodeSet
                    INNER JOIN TRAX_LOOKUP_CODE SubjectTypeLookup
                        ON TraxLookupCodeSet.PARENT_LOOKUP_CODE_ID = SubjectTypeLookup.ID
                    ON TraxLookupCode.LOOKUP_CODE_SET_ID = TraxLookupCodeSet.ID
                ON TraxTitleSubject.SUBJECT = TraxLookupCode.INTERNAL_VALUE
                AND TraxTitleSubject.SUBJECT_TYPE = SubjectTypeLookup.INTERNAL_VALUE            
            ON TraxTitle.ID = TraxTitleSubject.TITLE_ID
            AND TraxTitleSubject.PRIMARY_FLAG = 'Y'
        LEFT OUTER JOIN TRAX_CONTENT_OWNER TraxContentOwner
            ON TraxTitle.CONTENT_OWNER_ID = TraxContentOwner.ID
        ON PrmTargetGroupTitle.TITLE_ID = TraxTitle.ID
        INNER JOIN TRAX_ADDRESS TraxAddress
          ON TraxContentOwner.ID =  TraxAddress.CONTENT_OWNER_ID
GROUP BY PrmTargetGroupTitle.ID, 
       PrmTargetGroupTitle.PRM_TARGET_GROUP_ID, 
       PrmTargetGroupTitle.TITLE_ID, 
       PrmTargetGroupTitle.ADDED_DATE, 
       PrmTargetGroupTitle.REMOVED_DATE, 
       PrmTargetGroupTitle.CREATED_BY, 
       PrmTargetGroupTitle.CREATED_DATE, 
       PrmTargetGroupTitle.LAST_UPDATED_BY, 
       PrmTargetGroupTitle.LAST_UPDATED_DATE,
       TraxTitle.ID,
       TraxTitle.LONG_TITLE,
       TraxTitle.CATALOG_NUMBER,
       TraxTitle.OPEN_ACCESS,
       TraxTitle.TARGET_TRACKING_NO,
       TraxTitleSubject.SUBJECT,
       TraxLookupCode.DISPLAY_VALUE,
       TraxTitleSubject.SUBJECT_TYPE,
       TraxContentOwner.ID,
       TraxContentOwner.CONTENT_OWNER_NO,
       TraxContentOwner.NAME,
       TraxAddress.COUNTRY]]>
  </SQLQuery>
