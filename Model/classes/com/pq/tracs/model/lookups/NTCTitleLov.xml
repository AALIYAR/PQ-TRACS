  <SQLQuery>
    <![CDATA[select distinct nvl(currcat.id, ttl.id) AS TITLE_ID, 
       nvl(currcat.CONTENT_OWNER_ID, ttl.CONTENT_OWNER_ID) AS ACCOUNT_ID, 
       nvl(currcat.LONG_TITLE, ttl.LONG_TITLE) AS TITLE, 
       nvl(currcat.CATALOG_NUMBER, ttl.CATALOG_NUMBER) AS CATALOG_NUMBER, 
       nvl(currcat.CATALOG_ROOT, ttl.CATALOG_ROOT) AS CATALOG_ROOT,
       nvl(currcat.PUBLICATION_START_YEAR, ttl.PUBLICATION_START_YEAR) AS PUB_START_YEAR, 
       nvl(currcat.PUBLICATION_END_YEAR, ttl.PUBLICATION_END_YEAR) AS PUB_END_YEAR, 
       nvl(currcat.SHORT_TITLE, ttl.SHORT_TITLE) AS SHORT_TITLE, 
       nvl(currcat.FILM_AVAILABILITY, ttl.FILM_AVAILABILITY) AS FILM_AVAILABILITY,
       tco.NAME AS ACCOUNT_NAME,
       tco.SITE AS ACCOUNT_SITE,
       nvl(currcat.TARGET_TRACKING_NO, ttl.TARGET_TRACKING_NO) AS TRACKING_NO,
       tco.CONTENT_OWNER_NO AS ACCOUNT_NO,
       tcl.CLASSIFICATION_NUMBER
from trax_title ttl
  left outer join trax_title currcat
    on ttl.current_Catalog_id = currcat.id
  inner join trax_content_owner tco
    on nvl(currcat.content_owner_id, ttl.content_owner_id) = tco.id
  inner join trax_classification tcl
    on nvl(currcat.id, ttl.id) = tcl.title_id
    and tcl.primary_flag = 'Y'
where ttl.ACTIVE_FLAG = 'Y'
and ttl.CATALOG_NUMBER IS NOT NULL]]>
  </SQLQuery>
