   <SQLQuery>
      <![CDATA[SELECT Contact.ID,
    Contact.FIRST_NAME,
    Contact.MIDDLE_NAME,
    Contact.LAST_NAME,
    Contact.TITLE,
    Contact.GENDER,
    Contact.PRONUNCIATION,
    Contact.JOB_TITLE,
    Contact.JOB_ROLE,
    (concat(Contact.FIRST_NAME, concat(' ', Contact.LAST_NAME))) CONTACT_NAME,
    ContentOwnerContact.CONTENT_OWNER_ID
FROM TRAX_CONTACT Contact
    INNER JOIN TRAX_CONTENT_OWNER_CONTACT ContentOwnerContact
        ON Contact.ID = ContentOwnerContact.CONTACT_ID
WHERE ContentOwnerContact.CONTENT_OWNER_ID = :CONTENT_OWNER_ID]]>
   </SQLQuery>
